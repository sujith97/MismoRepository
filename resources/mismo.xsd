	</xsd:complexType>
	<xsd:complexType name="FLOOD_DETERMINATION_DETAIL">
		<xsd:sequence>
			<xsd:element name="FloodCertificationIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The certification number assigned by the compliance company when an order is received from the submitting / requesting party.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodDeterminationLifeOfLoanIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the flood contract with the compliance company is for the Life of the Loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodPartialIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that a portion of the subject property is in an identified Special Flood Hazard Area (SFHA).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodProductCertifyDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date on which the flood determination was assessed by the certification provider.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPCommunityIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The 6 digit NFIP community number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPCommunityFIRMDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date on which a community received its first Flood Insurance Rate Map. Used by Insurance companies to determine flood rates as to whether the structure is pre or post FIRM.&#13;
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPCommunityName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The complete name of the community in which the building or mobile home is located.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPCommunityParticipationStartDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date on which the community entered the Regular Program of the NFIP.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPCommunityParticipationStatusType" type="NFIPCommunityParticipationStatusTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A code indicating the type of flood program</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPCommunityParticipationStatusTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the NFIP Community Participation Status Type Other Description if Other is selected.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPCountyName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the county or counties in which the community is located. In unincorporated areas of a county, this would be classified as: unincorporated areas. For independent cities, this would be classified as: independent city.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPFloodDataRevisionType" type="NFIPFloodDataRevisionTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Letter of Map Amendment or Revision has been issued by FEMA.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPFloodZoneIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The flood zone covering the building or mobile home.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPLetterOfMapDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date the letter (LOMA /LOMR) was issued</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPMapIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The 11-digit number shown on the NFIP map that covers the building or mobile home. (Full concatenated version of the National Flood Community Identifier, NFIP Map Panel Identifier, NFIP Map Panel Suffix Identifier)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPMapIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the NFIP map covers the area where the building or mobile home is located.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPMapPanelIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The 4-digit number that describes the panel in which the property is located</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPMapPanelSuffixIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The letter that indicates the current map revision.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPMapPanelDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The map effective date or the map revised date shown on the NFIP map.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NFIPStateCode" type="MISMOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The state in which the community is located, the two-digit state abbreviation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProtectedAreaIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the building/mobile home is within a Coastal Barrier Resources Area (CBRA) or Otherwise Protected Area (OPA).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProtectedAreaDesignationDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date of the Coastal Barrier Resources Area (CBRA) or Otherwise Protected Area (OPA).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecialFloodHazardAreaIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When true, indicates if any portion of the building/mobile home is in an identified Special Flood Hazard Area (SFHA).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="FLOOD_REQUEST">
