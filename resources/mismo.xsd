				<xsd:annotation>
					<xsd:appinfo>Replaces RESPA_HUD_DETAIL container from V2</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HUD1_LINE_ITEMS" type="HUD1_LINE_ITEMS" minOccurs="0">
				<xsd:annotation>
					<xsd:appinfo>Replaces RESPA_HUD_DETAIL container from V2</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HUD1_SUMMARY" type="HUD1_SUMMARY" minOccurs="0"/>
