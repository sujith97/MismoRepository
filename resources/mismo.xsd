			<xsd:element name="PARTIES" type="PARTIES" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A collection of objects that define specific parties to a deal.  This includes the direct participating parties, such as borrower and seller and the indirect parties such as the credit report provifer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="POOL" type="POOL" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Holds data on multiple loans transmitted as a pool.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RELATIONSHIPS" type="RELATIONSHIPS" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Collection of xlink attributes that define relationships between two elments and optionally provide the type of relationship.  Many:Many relationships are formed with multiple RELATIONSHIP children with the same  xlink:title value</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SERVICER_REPORTING" type="SERVICER_REPORTING" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DEAL_SETS">
		<xsd:sequence>
			<xsd:element name="DEAL_SET" type="DEAL_SET" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="DEAL_SET_SERVICES" type="DEAL_SET_SERVICES" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A distinct set of services for bundles.  For example priceing a protfoloio of deals for servicing purchace or for collateral purchace.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PARTIES" type="PARTIES" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A collection of objects that define specific parties to a deal.  This includes the direct participating parties, such as borrower and seller and the indirect parties such as the credit report provifer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VERIFICATION_DATA" type="VERIFICATION_DATA" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DEAL_SET_SERVICE">
		<xsd:annotation>
			<xsd:documentation>Currently xsd:any to allow future definitions</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DEAL_SET_SERVICES">
		<xsd:annotation>
			<xsd:documentation>A distinct set of services for bundles.  For example priceing a protfoloio of deals for servicing purchace or for collateral purchace.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DEAL_SET_SERVICE" type="DEAL_SET_SERVICE" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Currently xsd:any to allow future definitions</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DECLARATION">
		<xsd:annotation>
			<xsd:documentation>Declarations that the borrower makes on the Uniform Residential Loan Application in section VIII.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DECLARATION_DETAIL" type="DECLARATION_DETAIL" minOccurs="0">
				<xsd:annotation>
					<xsd:appinfo>Additional information about the borrowers declarations.&#13;
</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DECLARATION_EXPLANATIONS" type="DECLARATION_EXPLANATIONS" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DECLARATION_DETAIL">
		<xsd:annotation>
			<xsd:appinfo>Additional information about the borrowers declarations.&#13;
</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AlimonyChildSupportObligationIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers declaration regarding obligations for alimony, child support, etc. Collected on the URLA in Section VIII line g.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankruptcyIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrower’s answer to the question: Have you been declared bankrupt within the past 7 years? Collected on the URLA in Section VIII, line b.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowedDownPaymentIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: Is any of the down payment borrowed? Collected on the URLA in Section VIII line h.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerFirstTimeHomebuyerIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the borrower qualifies as a first time homebuyer as determined by the lender and/or the investor. (Note: Information provided by the borrower in Section VIII, line m, of the URLA, regarding ownership of a property in the past three years, may not be relied upon for this information.) </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CitizenshipResidencyType" type="CitizenshipResidencyTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates this borrower's U.S. citizenship or alien status, as collected on the URLA (Section VIII, lines j. and k.).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CoMakerEndorserOfNoteIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: Are you a co-maker or endorser on a note? Collected on the URLA in Section VIII line i.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HomeownerPastThreeYearsType" type="HomeownerPastThreeYearsTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: Have you had an ownership interest in a property in the last three years?.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IntentToOccupyType" type="IntentToOccupyTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: Do you intend to occupy the property as your primary residence?</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanForeclosureOrJudgmentIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrower’s answer to the question: Have you directly or indirectly been obligated on any loan that resulted in foreclosure, transfer of title in lieu of foreclosure, or judgment? Collected on the URLA in Section VIII, line e.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OutstandingJudgmentsIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: Are there any outstanding judgments against you? Collected on the URLA in Section VIII line a.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PartyToLawsuitIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: Are you a party to a lawsuit? Collected on the URLA in Section VIII line d.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PresentlyDelinquentIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: Are you presently delinquent or in default on any Federal debt or any other loan mortgage, financial obligation, bond or loan guarantee? Collected on the URLA in Section VIII line f.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorPropertyTitleType" type="PriorPropertyTitleTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: How did you hold title to the home solely by yourself, jointly with your spouse, or jointly with another person? Collected on the URLA in Section VIII line m2.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorPropertyUsageType" type="PriorPropertyUsageTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: What type of property did you own principal residence, second home, or investment property? Collected on the URLA in Section VIII line m1.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PropertyForeclosedPastSevenYearsIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the question: Have you had property foreclosed upon or given title or deed in lieu thereof in the last seven years? Collected on the URLA in Section VIII line c.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DECLARATION_EXPLANATION">
		<xsd:annotation>
			<xsd:documentation>Additional information on declarations where the borrower may be required to provide additional details.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DeclarationExplanationDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture explanations to any yes answer to a declaration on lines a through i on the loan application. Captured on the URLA in a continuation sheet.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeclarationExplanationType" type="DeclarationExplanationTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the particular question asked in the declarations section of the loan application. Used in conjunction with the text field to capture additional information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeclarationExplanationTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to describe the kind of declaration if Other is selected as the Declaration Explanation Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DECLARATION_EXPLANATIONS">
		<xsd:sequence>
			<xsd:element name="DECLARATION_EXPLANATION" type="DECLARATION_EXPLANATION" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Additional information on declarations where the borrower may be required to provide additional details.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DELINQUENCY_NOTIFICATION">
		<xsd:annotation>
			<xsd:documentation>Container for all of the elements associated with a delinquency notification</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DELINQUENCY_NOTIFICATION_DETAIL" type="DELINQUENCY_NOTIFICATION_DETAIL" minOccurs="0"/>
			<xsd:element name="DELINQUENCY_NOTIFICATION_STATUSES" type="DELINQUENCY_NOTIFICATION_STATUSES" minOccurs="0"/>
			<xsd:element name="DELINQUENCY_NOTIFICATION_EVENTS" type="DELINQUENCY_NOTIFICATION_EVENTS" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DELINQUENCY_NOTIFICATION_DETAIL">
		<xsd:sequence>
			<xsd:element name="BankruptcySuspenseBalanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Unapplied funds related to a loan that is in bankruptcy</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DelinquencyEffectiveDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date that the delinquency of the loan started, could be at day 16 or day 30 depending on the Investor requirements.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DelinquencyReportingPropertyOverallConditionType" type="DelinquencyReportingPropertyOverallConditionTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to report the overall condition of the property associated with this loan as identified through a property inspector or through direct contact with Borrower, Neighbors, Real Estate Agent, etc.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DelinquencyReportingPropertyOverallConditionTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to describe the data source type if Other is selected as the Delinquency Reporting Property Overall Condition Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDelinquencyReasonType" type="LoanDelinquencyReasonTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The reason why the Borrower has become delinquent on the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDelinquencyReasonTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the Loan Delinquency Reason type if Other is selected.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PropertyCurrentOccupancyType" type="PropertyCurrentOccupancyTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the property occupancy status of a subject property.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RepaymentPlanDownPaymentAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The initial lump sum amount paid prior to the first monthly payment for the Repayment Plan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RepaymentPlanMonthlyDueAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Repayment Plan monthly payment amount. The amount of repayment scheduled to be paid each month under the Repayment Plan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RepaymentPlanTermMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The duration in months of a repayment plan agreement established with the borrower</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxAdvancesOnAccountsNotEscrowedIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the Servicer has incurred advances to pay delinquent taxes on a non-escrowed account.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DELINQUENCY_NOTIFICATION_EVENT">
		<xsd:annotation>
			<xsd:documentation>Container for all of the elements associated with a delinquency notification event.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="LoanDelinquencyEventDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date the loan delinquency event occurred.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDelinquencyEventType" type="LoanDelinquencyEventTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to report an event on a delinquent loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDelinquencyEventTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the Loan Delinquency Event Type if Other is selected.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DELINQUENCY_NOTIFICATION_EVENTS">
		<xsd:sequence>
			<xsd:element name="DELINQUENCY_NOTIFICATION_EVENT" type="DELINQUENCY_NOTIFICATION_EVENT" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Container for all of the elements associated with a delinquency notification event.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DELINQUENCY_NOTIFICATION_STATUS">
		<xsd:annotation>
			<xsd:documentation>Container for all of the elements associated with a delinquency notification status.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DelinquentPaymentCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A delinquent payment is an unpaid payment. This is a count of the currently unpaid paymnents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDelinquencyStatusDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date the loan delinquency status occurred.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDelinquencyStatusType" type="LoanDelinquencyStatusTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Loan Delinquency Status Type is used to report the status of a delinquent loan (i.e., is the loan in delinquency, foreclosure, bankruptcy, or some other status).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDelinquencyStatusTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the Loan Delinquency Status Type if Other is selected.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentDelinquencyStatusType" type="PaymentDelinquencyStatusTypeEnumeration" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Reports the delinquency status of the specific delinquency event.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DELINQUENCY_NOTIFICATION_STATUSES">
		<xsd:sequence>
			<xsd:element name="DELINQUENCY_NOTIFICATION_STATUS" type="DELINQUENCY_NOTIFICATION_STATUS" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Container for all of the elements associated with a delinquency notification status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DELINQUENCY_SUMMARY">
		<xsd:sequence>
			<xsd:element name="DelinquentPaymentCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A delinquent payment is an unpaid payment. This is a count of the currently unpaid paymnents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DelinquentPaymentsOverPastTwelveMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of times during the past twelve months that the payment on the subject loan was delinquent.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDelinquencyHistoryPeriodMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The duration in months of the period for which the history of the late mortgage is provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OnTimePaymentCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of on time payments since origination or the last delinquency.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentDelinquentDaysCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of days the borrower payment is currently past due.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalDelinquentInterestAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Sum of all past due interest from the last paid installment due date (DDLPI) to current date. It is computed based on scheduled amortization.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DEPENDENT">
		<xsd:annotation>
			<xsd:documentation>Information about the borrowers dependents.&#13;
</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DependentAgeYearsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The age of the dependent disclosed by the borrower. Collected on the URLA in Section III (Dependents Ages).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DEPENDENTS">
		<xsd:sequence>
			<xsd:element name="DEPENDENT" type="DEPENDENT" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Information about the borrowers dependents.&#13;
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DISCLOSURE_ON_SERVICER">
		<xsd:sequence>
			<xsd:element name="ServicerDaysOfTheWeekDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The normal business days of the week to contact Servicer with inquiries, as reflected on the Notice Of Assignment, Sale, Or Transfer Of Servicing Rights document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServicerDirectInquiryToDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The description of the party to contact with servicing related questions, as reflected on the Notice Of Assignment, Sale, Or Transfer Of Servicing Rights document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServicerHoursOfTheDayDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The normal business hours to contact Servicer with inquiries, as reflected on the Notice Of Assignment, Sale, Or Transfer Of Servicing Rights document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServicerInquiryTelephoneValue" type="MISMONumericString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The telephone number related to the description of the party to contact with servicing related questions, as reflected on the Notice Of Assignment, Sale, Or Transfer Of Servicing Rights document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServicerPaymentAcceptanceEndDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date that the current Servicer will stop accepting payments from the borrower for the mortgage loan, as reflected on the Notice Of Assignment, Sale, Or Transfer Of Servicing Rights document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServicerPaymentAcceptanceStartDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date that the new Servicer will start accepting payments from the borrower for the mortgage loan, as reflected on the Notice Of Assignment, Sale, Or Transfer Of Servicing Rights document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServicingTransferEffectiveDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date at which the transfer of servicing is effective as reflected on the Notice of Assignment, Sale, or Transfer of Servicing Rights document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransferOfServicingDisclosureType" type="TransferOfServicingDisclosureTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies detail about the party servicing the loan as stated on the Servicing Disclosure.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransferOfServicingDisclosureTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When Other is specified in Transfer Of Servicing Disclosure Type, this data element contains the description.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DISQUALIFICATION_REASON">
		<xsd:sequence>
			<xsd:element name="DisqualificationReasonText" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The reason that a loan does not qualify for a particular product.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DISQUALIFICATION_REASONS">
		<xsd:sequence>
			<xsd:element name="DISQUALIFICATION_REASON" type="DISQUALIFICATION_REASON" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENT">
		<xsd:annotation>
			<xsd:documentation>Information needed to represent a single complex electronic document. It allows transport of any version 3.x DOCUMENT.  Its children allow unified delivery of the document rendered in a particular viewing technology such as HTML or PDF.  The data for the rendered content is also present along with a MAP of the data to rendered position.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice minOccurs="0">
				<xsd:element name="REFERENCE" type="REFERENCE" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Used to identify the alternate location of content. Used to reduce the redundancy of a message by placing common content in a alternative location and replacing it with the REFERENCE element that points to that location. A message recipient MAY replace the content of the referenced location for the REFERENCE element before processing or may process the information from that location. Processing XML encryption or XML signature will ALWAYS require replacement of the REFERENCE element with the content or the referenced location. The referenced location MAY be internal to the current XML document or it MAY be external. When internal there WILL NOT be a LocationURI child element. The referenced content MAY or MAY NOT be located at a place other than the root element of the external location.  If it is NOT the root element the LocationXPath child element identifies the location within the located XML document. For an external reference the LocationXPath is ALWAYS and absolute path from the document root. For and internal reference the LocationXPath child element value MAY absolute of relative.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence minOccurs="0">
					<xsd:choice minOccurs="0">
						<xsd:element name="UNKNOWN_VERSION3_DOCUMENT" type="UNKNOWN_VERSION3_DOCUMENT" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Used as a place to carry any V3.x DOCUMENT that is of a differnent MISMO Version than that of the MESSAGE</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence minOccurs="0">
							<xsd:element name="AUDIT_TRAIL" type="AUDIT_TRAIL" minOccurs="0"/>
							<xsd:element name="DEAL_SETS" type="DEAL_SETS" minOccurs="0"/>
							<xsd:element name="MAP" type="MAP" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>The Map Element contains or points to stylesheets for generating or populating document views based on the contents of the DATA and/or SIGNATORY elements. The resulting View is used to replace a View containing outdated data or to verify that the data in the target View matches the data provided in the SIGNATORY and DATA elements.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="RELATIONSHIPS" type="RELATIONSHIPS" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Collection of xlink attributes that define relationships between two elments and optionally provide the type of relationship.  Many:Many relationships are formed with multiple RELATIONSHIP children with the same  xlink:title value</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="SIGNATORIES" type="SIGNATORIES" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Contains information regarding the types of electronic signatures used to represent the legally binding signatures of the parties named in the document along with information regarding the geographic location of the signing event(s).</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="SYSTEM_SIGNATURES" type="SYSTEM_SIGNATURES" minOccurs="0"/>
							<xsd:element name="VIEWS" type="VIEWS" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>The VIEWS element contains a list of VIEW elements.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:choice>
					<xsd:element name="ABOUT_VERSIONS" type="ABOUT_VERSIONS" minOccurs="0"/>
					<xsd:element name="DOCUMENT_CLASSIFICATION" type="DOCUMENT_CLASSIFICATION" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Contains information about a specific instance of a document that describes its use (as opposed to its contents).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
				</xsd:sequence>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="MISMOReferenceModelIdentifier" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>The MISMO Reference Model Identifier is a unique value that represents the version of the MISMO reference model to which the containing XML instance document complies. For detailed information, please see MISMO Engineering Guide 006 – “Versioning and Release Schedule”, at http://wiki.mismo.org/MISMO.Wiki/Wiki.jsp?page=Meg0006</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENTATION">
		<xsd:annotation>
			<xsd:documentation>A piece of documentation about borrower Asset, Income or employment.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AssetDocumentationLevelIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the extent or level of the asset documentation being collected to process and underwrite the loan</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AssetDocumentationType" type="AssetDocumentationTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of documentation used as a means of identifying or verifying assets used in the transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AssetDocumentationTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the Asset Documentation Type if Other is selected. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AssetVerificationRangeCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of time periods as defined by the Verification Range Type for which documentation is collected .</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AssetVerificationRangeType" type="VerificationRangeTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the period or range of time for which the specific type of Documentation Type is collected.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AssetVerificationRangeTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> A free form text field used to collect additional information when other is selected for Verification Range Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentationStateType" type="DocumentationStateTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the state of the documention associated with the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentationStateTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for Documentation State Type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentDocumentationLevelIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the extent or level of the employment documentation being collected to process and underwrite the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentDocumentationType" type="EmploymentDocumentationTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> The type of documentation used as a means of identifying or verifying the borrowers employment used in the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentDocumentationTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the Employment Documentation Type if Other is selected</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentVerificationRangeCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of time periods as defined by the Verification Range Type for which documentation is collected .</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentVerificationRangeType" type="VerificationRangeTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the period or range of time for which the specific type of Documentation Type is collected.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentVerificationRangeTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> A free form text field used to collect additional information when other is selected for Verification Range Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncomeDocumentationLevelIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> Identifies the extent or level of the income documentation being collected to process and underwrite the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncomeDocumentationType" type="IncomeDocumentationTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of documentation used as a means of identifying or verifying the borrowers income used in the transaction&#13;
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncomeDocumentationTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the Income Documentation Type if Other is selected</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncomeVerificationRangeCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of time periods as defined by the Verification Range Type for which documentation is collected .</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncomeVerificationRangeType" type="VerificationRangeTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the period or range of time for which the specific type of Documentation Type is collected.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncomeVerificationRangeTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> A free form text field used to collect additional information when other is selected for Verification Range Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENTATIONS">
		<xsd:annotation>
			<xsd:documentation>Collection of the documentation used to verifiy borrower information about Assets, Income and Employment</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DOCUMENTATION" type="DOCUMENTATION" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A piece of documentation about borrower Asset, Income or employment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENTS">
		<xsd:sequence>
			<xsd:element name="DOCUMENT" type="DOCUMENT" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Information needed to represent a single complex electronic document. It allows transport of any version 3.x DOCUMENT.  Its children allow unified delivery of the document rendered in a particular viewing technology such as HTML or PDF.  The data for the rendered content is also present along with a MAP of the data to rendered position.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENT_CLASS">
		<xsd:annotation>
			<xsd:documentation>Contains doucment type information.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DocumentSubtypeType" type="DocumentSubtypeTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The SubtypeDescription should be one of the applicable values for Document Information Subtype Description defined in the Document Data Dictionary as associated with the Document Information Type of the document. New descriptions should conform to the guidelines for creating the Subtype names.</xsd:documentation>
					<xsd:appinfo>Renamed.  Used to be "Document Subtype Description".</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentSubtypeTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for Document Subtype.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentSupertypeType" type="DocumentSupertypeTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allows trading partners to classify documents at a broader level than required by the more granular Types and Subtypes, as appropriate for the business purpose. There is only one SuperType assigned to each type. This can be sent as an alternate to sending Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentType" type="DocumentTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies a specific document defined at a granular level.  May be modified or further constrained by other Document data to make the document instance more specific. Further defined in the MISMO Document Classification I-Guide.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for Document Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecordingJurisdictionDocumentCode" type="MISMOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Abbreviated code used by recorder in indexing software. Specific to County document is being recorded in</xsd:documentation>
					<xsd:appinfo>Renamed. Used to be PRIA Document Code.</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENT_CLASSES">
		<xsd:annotation>
			<xsd:documentation>Contains repeatable information about the type of document being described. Is used to address multi-title documents.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DOCUMENT_CLASS" type="DOCUMENT_CLASS" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Contains doucment type information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENT_CLASSIFICATION">
		<xsd:annotation>
			<xsd:documentation>Contains information about a specific instance of a document that describes its use (as opposed to its contents).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DOCUMENT_CLASSES" type="DOCUMENT_CLASSES" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Contains repeatable information about the type of document being described. Is used to address multi-title documents.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DOCUMENT_CLASSIFICATION_DETAIL" type="DOCUMENT_CLASSIFICATION_DETAIL" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Contains non-repeatable information about a specific instance of a document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENT_CLASSIFICATION_DETAIL">
		<xsd:annotation>
			<xsd:documentation>Contains non-repeatable information about a specific instance of a document.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AcceptableSigningMethodType" type="AcceptableSigningMethodTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is an instruction as to the signing methods that are acceptable for this document. It not reflect a state in time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentCommentDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A text field for any information or instructions regarding a document that a preparer includes to assist the recipient with understanding the contents or processing rules.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentFormIssuingEntityNumberIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The identifier assigned by the organization that defined the format of the document.  Form ID as commonly used; generally would not include the issuing entity as that would be duplicative, unless it doesn't make business sense to exclude it, for example, the HUD-1.</xsd:documentation>
					<xsd:appinfo>Renamed. Used to be Document Form Number Identifier.</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentFormIssuingEntityVersionIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The version identifier assigned by the organization that defined the format of the document.  Could be date, number; whatever the issuing agency determines.</xsd:documentation>
					<xsd:appinfo>Renamed. Use to be Document Form Version Identifier.</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentFormIssuingEntityNameType" type="DocumentFormIssuingEntityNameTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The identity of the organization that defined the format of the document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentFormIssuingEntityNameTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The state, county,  municipality or foreign country that requires the use of or issues the document. Example 1: A borrower residing in MN is required to receive this document. MN would be the jurisdiction. If it is a state requirement, include only the state abbreviation, if it is a county or municipality requirement, include both the county or municipality name and the state abbreviation.  An enumerated list is included in the I-Guide. Example 2:  The state issuing a driver's license or country issuing a passport.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentFormPublisherEntityName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the organization/company that produced/provided/sold the form/content. Example: VMP, Greatland.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentFormPublisherNumberIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The form name/number used by the organization/company that produced/provided/sold the form/content.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentFormPublisherVersionIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The version date or other identifier used by the organization/company that produced/provided/sold the form/content.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentHasBeenRecordedIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that a document has been recorded in the public records</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The specific title of the document as provided on the document.  The name or label that is used for this document. It may but is not always the same as the Document Type. For example, the promissory note for a particiular loan program may be called "Gold Star Customer Agreement."</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentNegotiableInstrumentIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This indicates, that when fully executed, the document is a negotiable instrument.  Only the original or authoriative copy is the negotiable instrument, but copies would still carry this indicator if it were applied before and electronic version was sealed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentPeriodEndDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The PeriodEndDate is used for the ending date of the period covered in the document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentPeriodStartDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The PeriodStartDate is used for the starting date of the period covered in the document or the period covered in the document.  For documents that cover an exact period, e.g. a year or a year and month, only provide those portions of the date. In the instance of an IRS tax document this is the year indicated on the Tax document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentReceiptDatetime" type="MISMODatetime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date and time that the document was received.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentRecordationProcessingType" type="DocumentRecordationProcessingTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Describes whether the document is one that is/was required to be recorded to be fully effective.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PageCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The total number of pages in a document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENT_MANAGEMENT">
		<xsd:sequence>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENT_SET">
		<xsd:sequence>
			<xsd:element name="DOCUMENTS" type="DOCUMENTS" minOccurs="0"/>
			<xsd:element name="FOREIGN_OBJECTS" type="FOREIGN_OBJECTS" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DOCUMENT_SETS">
		<xsd:sequence>
			<xsd:element name="DOCUMENT_SET" type="DOCUMENT_SET" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="FOREIGN_OBJECTS" type="FOREIGN_OBJECTS" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A collection of FORREIGN_OBJECT elements</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DOWN_PAYMENT">
		<xsd:annotation>
			<xsd:documentation>Information specific to portion of the sales prcie paid by a seller to a buyer to close a sales transaction.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DownPaymentAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of the borrower’s Down Payment Type. Collected on the URLA in Section II (Source of Down Payment).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DownPaymentOptionType" type="DownPaymentOptionTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the type of option used to facilitate the borrowers down payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DownPaymentOptionTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for Down Payment Option Type.</xsd:documentation>
					<xsd:appinfo> </xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DownPaymentSourceType" type="DownPaymentSourceTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the entity providing funds for the down payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DownPaymentSourceTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for Down Payment Source Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DownPaymentType" type="DownPaymentTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the general names (type) of items commonly used for a down payment by the borrower(s) in a mortgage loan transaction. This may be collected on the URLA in Section II (Source of Down Payment).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DownPaymentTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other Type of Down Payment is selected for Down Payment Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PropertySellerFundingIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether some or all of the down payment was funded by the Property Seller either directly or indirectly through a community or religious non profit.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DOWN_PAYMENTS">
		<xsd:sequence>
			<xsd:element name="DOWN_PAYMENT" type="DOWN_PAYMENT" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Information specific to portion of the sales prcie paid by a seller to a buyer to close a sales transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DRAW">
		<xsd:annotation>
			<xsd:documentation>Information on the disbursement of a portion of the loan proceeds to the borrower. This container would be used for a loan with a line of credit or a construction loan.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DRAW_ACTIVITIES" type="DRAW_ACTIVITIES" minOccurs="0"/>
			<xsd:element name="DRAW_RULE" type="DRAW_RULE" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DRAW_ACTIVITIES">
		<xsd:sequence>
			<xsd:element name="DRAW_ACTIVITY" type="DRAW_ACTIVITY" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="DRAW_ACTIVITY">
		<xsd:sequence>
			<xsd:element name="LoanDrawAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount borrowed against the loan for the draw transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date on which the draw transaction was executed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanTotalDrawAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The total amount of all draws that the borrower has made as of the date reported.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DRAW_RULE">
		<xsd:sequence>
			<xsd:element name="LoanDrawExpirationDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The last date on which the borrower is able to draw funds on a line of credit.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawExtensionTermMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of months by which the draw term can be extended in a single transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawMaximumAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The maximum dollar amount a borrower can withdraw in each draw transaction against the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawMaximumTermMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The maximum number of months to which the draw term can be extended.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawMinimumAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The minimum dollar amount a borrower can withdraw in each draw transaction against the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawMinimumInitialDrawAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The minimum dollar amount the borrower is required to withdraw in the first draw transaction against the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawPeriodMaximumDrawCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The maximum number of times a borrower can withdraw funds against the loan during a specific period.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawPeriodMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The length of a time period in months during which the borrower may withdraw funds from the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawStartPeriodMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The minimum number of months from closing after which the borrower may begin to withdraw funds from the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanDrawTermMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The total number of months during which draws are allowed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="ELECTRONIC_SIGNATURE">
		<xsd:annotation>
			<xsd:documentation>Container for specific details relating to the use of the various electronic signature types supported.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="FOREIGN_OBJECT" type="FOREIGN_OBJECT" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to hold content that is not part of the namespace or version of the MISMO V3 model  It is either contained directly (via EmbeddedContentXML) or indirectly (via LocationURI). A FOREIGN_OBJECT can be Encoded in a variety of manners without changing its underlying meaning. However, the Encoding of EmbeddedContentXML MUST result in a schema-valid representation. The representation and Encoding are the only transformations supported by the model.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:choice>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="ELIGIBLE_LOAN_PRODUCTS">
		<xsd:sequence>
			<xsd:element name="ELIGIBLE_LOAN_PRODUCT" type="LOAN_PRODUCT" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A collection of facts used as descriptors of the characteristics of a financial instrument for sale in the marketplace that an investor is willing to buy.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="EMPLOYER">
		<xsd:annotation>
			<xsd:documentation>Information about the borrowers current and previous employers including name, address, contact information, time on job and type of employment. Also includes information about the verification status of the employment and credit comments relating to the employment.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="INDIVIDUAL" type="INDIVIDUAL" minOccurs="0"/>
				<xsd:element name="LEGAL_ENTITY" type="LEGAL_ENTITY" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>This is additional information that describes or identifies a legal entity.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="ADDRESS" type="ADDRESS" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Collection of Address related data points that are used in many contexts.The Collection of Address related data points that are used in many contexts.&#13;
The primary use case for this container is to support mailling material to the address.&#13;
V2 ADDRESS container was implicitly a mailing container, V3 will be explicitly for mail.&#13;
With in this use case there are sub cases.&#13;
1. Transfer of information so that the recipient may mail to a personal address as a single piece of mail&#13;
2. Transfer of information so that the recipient may send a package by mean including, but not restricted to the USPS&#13;
3. Transfer of information so that the recipient may include the address in a bulk mailing to a personal address&#13;
4. Transfer of information so that the recipient may mail a single piece of may to a business&#13;
&#13;
Address is based upon USPS publication 28.&#13;
Pub-28 has two sections, format of an address on a piece of mail and data points that MAY compose an address.&#13;
The data point noun children of ADDRESS are the union components of an address on a piece of mail or a package.  The reciever of an address that uses it to format a mail label would need to appliy the formating requirements of pub-28.&#13;
&#13;
For example: Pub 28 refers to the address "last line" as being composed of the City &lt;&lt;single space&gt;&gt;State&lt;&lt;single space&gt;&gt;ZIP. Insertion of the space seperators would be the responsibility of the mail sender system. The ADDRESS container holds the City, State and Postal code values.&#13;
&#13;
Postal Code vs ZIP CODE&#13;
MISMO V2 used Postal Code, to to represent the possibility of the mail recipient being at an address outside the US. We will continue that distinction.&#13;
&#13;
Pub-28 defines a mail format that is complient with the international mailing standard defined by the UPU.  Pub-28 points out that for mail that crosses internationl borders the format of the sending country should be used. The USPS international format is to have COUNTRY as the last line of the address on the piece of mail.  The ADDRESS container has a noun child data point for Country. It is the responiblity of the user of the data to plac to correct values on the piece of mail correctly.&#13;
&#13;
NEED TO IDENTIFY THE NAME OF THE FIELDS IN THE PUB 28 WHEN MISMO CHOOSES TO DIVERT&#13;
&#13;
Include information regarding if...then on using unparsed vs. parsed information&#13;
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CREDIT_COMMENTS" type="CREDIT_COMMENTS" minOccurs="0"/>
			<xsd:element name="EMPLOYMENT" type="EMPLOYMENT" minOccurs="0">
				<xsd:annotation>
					<xsd:appinfo>Additional information about the employer.&#13;
</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EMPLOYMENT_DOCUMENTATIONS" type="EMPLOYMENT_DOCUMENTATIONS" minOccurs="0"/>
			<xsd:element name="VERIFICATION" type="VERIFICATION" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Information arround the verification of a particular item (e.g. asset, income, employment, rent, etc).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="EMPLOYERS">
		<xsd:sequence>
			<xsd:element name="EMPLOYER" type="EMPLOYER" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Information about the borrowers current and previous employers including name, address, contact information, time on job and type of employment. Also includes information about the verification status of the employment and credit comments relating to the employment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="EMPLOYMENT">
		<xsd:annotation>
			<xsd:appinfo>Additional information about the employer.&#13;
</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EmployedAbroadIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the borrower’s physical place of employment is located outside of the United States. If the borrower's employer is a foreign entity and the borrower's place of employment is located inside the United States this value would be No.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentBorrowerHomeOfficeIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that in the referenced employment, the office of the borrower is within the primary residence.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentBorrowerSelfEmployedIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that in the referenced employment the borrower is self-employed.</xsd:documentation>
					<xsd:appinfo>Removed reference to URLA from definition.</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentClassificationType" type="EmploymentClassificationTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether the employment for the borrower is primary or secondary.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentEndDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date that the borrower ended the employment position with the employer. Current employment would have no end data. One source of this data is the URLA in Section IV (Dated To).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentMonthlyIncomeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount per month of Income associated with the borrowers employment. Note: For current secondary or previous primary or previous secondary income.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentMonthsOnJobCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of complete months of service to an employer from the start of employment until the date on which the employment is reported.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentPositionDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An expanded description of the borrowers employment position. Collected on the URLA in Section IV.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentReportedDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the date when the employment information was reported to the repository bureau.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentStartDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date that the borrower started the employment position with the employer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentStatusType" type="EmploymentStatusTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether the employment for the borrower is current or previous.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentTimeInLineOfWorkMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of complete months the borrower has been employed in the reported occupation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentTimeInLineOfWorkYearsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The total number of years the borrower has been employed in thet reported occupation. One source of this data is the URLA Section IV (Years Employed In This Line Of Work.)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentYearsOnJobCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of complete years of service to an employer from the start of employment until the date on which the employment is reported.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecialBorrowerEmployerRelationshipType" type="SpecialBorrowerEmployerRelationshipTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies a unique category of employer which is the source of income for the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecialBorrowerEmployerRelationshipTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for Borrower Employer Relationship Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="EMPLOYMENT_DOCUMENTATION">
		<xsd:annotation>
			<xsd:documentation>Contains information about documents that may be associated with the income.&#13;
</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DocumentationStateType" type="DocumentationStateTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the state of the documention associated with the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentationStateTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for Documentation State Type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentDocumentationType" type="EmploymentDocumentationTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> The type of documentation used as a means of identifying or verifying the borrowers employment used in the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentDocumentationTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the Employment Documentation Type if Other is selected</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentVerificationRangeCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of time periods as defined by the Verification Range Type for which documentation is collected .</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentVerificationRangeType" type="VerificationRangeTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the period or range of time for which the specific type of Documentation Type is collected.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentVerificationRangeTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> A free form text field used to collect additional information when other is selected for Verification Range Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="EMPLOYMENT_DOCUMENTATIONS">
		<xsd:sequence>
			<xsd:element name="EMPLOYMENT_DOCUMENTATION" type="EMPLOYMENT_DOCUMENTATION" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Contains information about documents that may be associated with the income.&#13;
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="ESCROW">
		<xsd:annotation>
			<xsd:documentation>Information specific to funds collected through the loan payment and held to pay expenses such as taxes and insurance.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ESCROW_DETAIL" type="ESCROW_DETAIL" minOccurs="0"/>
			<xsd:element name="ESCROW_DISCLOSURES" type="ESCROW_DISCLOSURES" minOccurs="0"/>
			<xsd:element name="ESCROW_ITEMS" type="ESCROW_ITEMS" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="ESCROW_DETAIL">
		<xsd:sequence>
			<xsd:element name="EscrowAccountInitialBalanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The initial total of all escrow amounts collected to establish the escrow account.</xsd:documentation>
					<xsd:appinfo>Replaces "Account Detail Initial Balance Amount" from V2</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowAccountMinimumBalanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The lowest escrow balance target amount determined by the allowable cushion.</xsd:documentation>
					<xsd:appinfo>Replaces "Escrow Account Detail Minimum Balance Amount" from V2</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowAggregateAccountingAdjustmentAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of any adjustment made to total escrow account based on Regulation X requirements. This must be a negative number or 0.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowBalanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Escrow balance on the loan net of any escrow advances. May be a negative amount.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowCompletionFundsBalanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount of the FHA 203K or rehab funds held in escrow required for the completion of the rehabilitation or repair of the subject property.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowCushionNumberOfMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Denotes the maximum number of monthly escrow collections allowed to be held as the projected lowest balance in the escrow account for the projection year. Escrow cushion months will have a value of 0, 1 or 2.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowHoldbackAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Amount held back at closing by the lender or escrow agent until a particular condition has been met.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InitialEscrowProjectionStatementDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the date that the initial escrow statement was given to the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InterestOnEscrowAccruedAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies Interest on Escrow accrued not yet posted amount.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InterestOnEscrowAccruedThroughDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies date thru which interest on escrow has accrued</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InterestOnEscrowBackupWithholdingIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag to indicate if interest on escrow s/b withheld due to IRS notification.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InterestOnEscrowIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies if interest is paid on borrower's Escrow Account</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InterestOnEscrowYearToDatePostedAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the dollar amount of interest on escrow posted for the current year.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InterestOnRestrictedEscrowIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies if interest is paid on restricted escrow</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastEscrowAnalysisDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the date of last Escrow Analysis</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastInterestOnEscrowPostedDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the date for which interest on escrow was last posted to the account</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MIEscrowIncludedInAggregateIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether Private Mortgage Insurance Escrow is to be included in the cushion for Aggregate Accounting.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RestrictedEscrowBalanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Loss Draft Funds Balance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="ESCROW_DISCLOSURE">
