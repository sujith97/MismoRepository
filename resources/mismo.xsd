					<xsd:documentation>The number of bedrooms per unit.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PropertyDwellingUnitEligibleRentAmount" type="MISMOAmount" minOccurs="0">
