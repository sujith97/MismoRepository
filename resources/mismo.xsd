					<xsd:appinfo>Formerly "REO Property Lien UPB Amount" in Version 2.x</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OwnedPropertyMaintenanceExpenseAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The monthly expense amount for property taxes, maintenance and insurance on the owned real property.</xsd:documentation>
					<xsd:appinfo>Formerly "REO Property Maintenance Expense Amount" in Version 2.x.</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OwnedPropertyMarketValueAmount" type="MISMOAmount" minOccurs="0">
