			<xsd:documentation>Information specific to the Flood Zone Determination pursuant to the National Flood Insurance Program.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FLOOD_DETERMINATION_DETAIL" type="FLOOD_DETERMINATION_DETAIL" minOccurs="0"/>
			<xsd:element name="FLOOD_RISK_FACTOR" type="FLOOD_RISK_FACTOR" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
