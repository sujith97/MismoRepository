					<xsd:documentation>The description of the RESPA Fee item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaidToType" type="RESPA_FeePaidToTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the category or type of payee to which the fee will be paid</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaidToTypeOtherDescription" type="MISMOString" minOccurs="0">
