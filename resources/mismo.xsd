	<xsd:simpleType name="SignatureFieldMarkTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies whether the signature field is for a full signature or for an initials mark.</xsd:documentation>
			<xsd:documentation>term: Signature Field Mark Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="FullSignature">
				<xsd:annotation>
					<xsd:documentation>Denotes that the signature field is a for a full signature</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Initials">
				<xsd:annotation>
					<xsd:documentation>Denotes that the signature field is for an initials mark.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SignatureTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Contains the signature type proposed by the document creator. The actual type of the signature applied to the signature field may be different (please see audit trail)</xsd:documentation>
			<xsd:documentation>The ActualSignatureType contains the actual type of the signature applied to the signature field.</xsd:documentation>
			<xsd:documentation>term: Signature Type</xsd:documentation>
			<xsd:documentation>term: Actual Signature Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Text">
				<xsd:annotation>
					<xsd:documentation>Denotes a text signature. The signer types his/her name (signature) in the signature field or clicks a button onscreen indicating acceptance or agreement.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Image">
				<xsd:annotation>
					<xsd:documentation>Denotes an image signature. A graphic image of the signers signature  is captured and placed in signature field.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Digital">
				<xsd:annotation>
					<xsd:documentation>Denotes a digital signature. The signer uses a digital certificate to apply a PKI-based signature to the document. The certificate maybe a personal certificate of the signer, a system certificate, or a dynamically generated certificate created for that transaction. After signing a signature appearance is generated and place on the signature field in the view. </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>Denotes that the signature type is other than the ones listed above. When this option is selected, a description of the signature type must be provided in the SignatureTypeOtherDescription attribute.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SkipPaymentActionTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the action to be taken when a skip payment feature is exercised.</xsd:documentation>
			<xsd:documentation>term: Skip Payment Action Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CapitalizeInterest">
				<xsd:annotation>
					<xsd:documentation>Interest from the skipped payment is capitalized into the UPB.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CreateSubordinateLoanComponent">
				<xsd:annotation>
					<xsd:documentation>Principal and interest from the skipped payment is used to create a subordinate loan component.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ExtendTerm">
				<xsd:annotation>
					<xsd:documentation>The mortgage term is extended to amortize the skipped payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MakeupPayment">
				<xsd:annotation>
					<xsd:documentation>The borrower makes up the skipped payment with an unscheduled payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SpecialBorrowerEmployerRelationshipTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies a unique category of employer which is the source of income for the borrower.</xsd:documentation>
			<xsd:appinfo>  AUS  Enumerated   AUS  Special Borrower Employer Relationship Type </xsd:appinfo>
			<xsd:documentation>term: Special Borrower Employer Relationship Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PropertySeller"/>
			<xsd:enumeration value="RealEstateBroker"/>
			<xsd:enumeration value="EmployedByRelative"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StopCodeActionTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates special handling action to be taken.</xsd:documentation>
			<xsd:documentation>term: Stop Code Action Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AcceptCertifiedFundsOnly"/>
			<xsd:enumeration value="DoNotApplyNormalPayments"/>
			<xsd:enumeration value="DoNotAssessLateCharge"/>
			<xsd:enumeration value="DoNotDisburseFromEscrow"/>
			<xsd:enumeration value="DoNotPerformEscrowAnalysis"/>
			<xsd:enumeration value="DoNotSendDelinquentNotices"/>
			<xsd:enumeration value="DoNotSolicitForOptionalProducts"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="SpecialHandling"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StopCodeConditionTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates the loan condition that is related to the stop code action type for the loan.</xsd:documentation>
			<xsd:documentation>term: Stop Code Condition Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BadCheckHoldingNSF"/>
			<xsd:enumeration value="LoanInBankruptcy"/>
			<xsd:enumeration value="LoanInForeclosure"/>
			<xsd:enumeration value="LoanPaidInFull"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PayoffPending"/>
			<xsd:enumeration value="ProcessingException"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SummaryAmountTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the type of summary amount.</xsd:documentation>
			<xsd:appinfo> Enumerated Summary  AUS SEC  Summary Amount Type </xsd:appinfo>
			<xsd:documentation>term: Summary Amount Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SubtotalLiabilitiesForRentalPropertyBalance"/>
			<xsd:enumeration value="SubtotalLiabilitiesForRentalPropertyMonthlyPayment"/>
			<xsd:enumeration value="SubtotalLiabilitiesMonthlyPayment"/>
			<xsd:enumeration value="SubtotalLiabilitiesPaidByClosingNotIncludingSubjectPropertyLiensBalance"/>
			<xsd:enumeration value="SubtotalLiabilitiesPaidByClosingNotIncludingSubjectPropertyLiensMonthlyPayment"/>
			<xsd:enumeration value="SubtotalLiquidAssetsNotIncludingGift"/>
			<xsd:enumeration value="SubtotalNonLiquidAssets"/>
			<xsd:enumeration value="SubtotalOmittedLiabilitiesBalance"/>
			<xsd:enumeration value="SubtotalOmittedLiabilitiesMonthlyPayment"/>
			<xsd:enumeration value="SubtotalResubordinatedLiabilitiesBalanceForSubjectProperty"/>
			<xsd:enumeration value="SubtotalResubordinatedLiabilitiesMonthlyPaymentForSubjectProperty"/>
			<xsd:enumeration value="SubtotalSubjectPropertyLiensPaidByClosingBalance"/>
			<xsd:enumeration value="SubtotalSubjectPropertyLiensPaidByClosingMonthlyPayment"/>
			<xsd:enumeration value="TotalLiabilitiesBalance"/>
			<xsd:enumeration value="TotalMonthlyIncomeNotIncludingNetRentalIncome"/>
			<xsd:enumeration value="TotalPresentHousingExpense"/>
			<xsd:enumeration value="UndrawnHELOC">
				<xsd:annotation>
					<xsd:documentation>The amount of the unused HELOC remaining.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TaxAuthorityTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the type of tax to be paid</xsd:documentation>
			<xsd:appinfo> Enumerated Escrow Tax Item  Servicing Servicing Transfer SEC  Tax Authority Type </xsd:appinfo>
			<xsd:documentation>term: Tax Authority Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AnnualCityBondAuthorityTax"/>
			<xsd:enumeration value="AnnualCountyBondAuthorityTax"/>
			<xsd:enumeration value="AssessmentDistrictTax"/>
			<xsd:enumeration value="BondAuthorityTax"/>
			<xsd:enumeration value="BoroughTax"/>
			<xsd:enumeration value="CentralAppraisalTaxingAuthorityTax"/>
			<xsd:enumeration value="CentralCollectionTaxingAuthorityTax"/>
			<xsd:enumeration value="CityTax"/>
			<xsd:enumeration value="CityAndSchoolTax"/>
			<xsd:enumeration value="CountyTax"/>
			<xsd:enumeration value="CountyCollectedByOtherTaxingAuthorityTax"/>
			<xsd:enumeration value="FireOrPoliceTax"/>
			<xsd:enumeration value="HomeownersAssociationTax"/>
			<xsd:enumeration value="HospitalsTax"/>
			<xsd:enumeration value="JuniorCollegesTax"/>
			<xsd:enumeration value="MobileHomeAuthorityTax"/>
			<xsd:enumeration value="OtherTax"/>
			<xsd:enumeration value="OtherBondAuthorityTax"/>
			<xsd:enumeration value="OtherCombinationCollectionTax"/>
			<xsd:enumeration value="OtherMunicipalServicesTax"/>
			<xsd:enumeration value="OtherMunicipalityTax"/>
			<xsd:enumeration value="OtherSchoolsTax"/>
			<xsd:enumeration value="OtherSpecialApplicationsTax"/>
			<xsd:enumeration value="OtherSpecialDistrictsTax"/>
			<xsd:enumeration value="ReclamationDistrictTax"/>
			<xsd:enumeration value="RoadsOrBridgesTax"/>
			<xsd:enumeration value="SanitationTax"/>
			<xsd:enumeration value="SchoolDistrictTax"/>
			<xsd:enumeration value="SemiannualCityBondAuthorityTax"/>
			<xsd:enumeration value="SemiannualCountyBondAuthorityTax"/>
			<xsd:enumeration value="SpecialDistrictTax"/>
			<xsd:enumeration value="StateAndCountyTax"/>
			<xsd:enumeration value="StateTaxingAuthorityTax"/>
			<xsd:enumeration value="TownTax"/>
			<xsd:enumeration value="TownshipTax"/>
			<xsd:enumeration value="TownshipAndCountyTax"/>
			<xsd:enumeration value="UnsecuredCountyTaxes"/>
			<xsd:enumeration value="UtilityDistrictTax"/>
			<xsd:enumeration value="VillageTax"/>
			<xsd:enumeration value="WaterControlTax"/>
			<xsd:enumeration value="WaterOrSewerDistrictTax"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TaxpayerIdentifierTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the type of identification number used by the Internal Revenue Service (IRS) in the administration of tax laws. It is issued either by the Social Security Administration (SSA) or the IRS. A Social Security number (SSN) is issued by the SSA; all other taxpayer identification numbers are issued by the IRS.</xsd:documentation>
			<xsd:documentation>term: Taxpayer Identifier Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="EmployerIdentificationNumber"/>
			<xsd:enumeration value="IndividualTaxpayerIdentificationNumber"/>
			<xsd:enumeration value="PreparerTaxpayerIdentificationNumber"/>
			<xsd:enumeration value="SocialSecurityNumber"/>
			<xsd:enumeration value="TaxpayerIdentificationNumberForPendingUSAdoptions"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TitleAssociationTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the national title association.</xsd:documentation>
			<xsd:documentation>term: Title Association Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ALTA">
				<xsd:annotation>
					<xsd:documentation>American Land Title Association</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CLTA"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="TLTA"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TitleEndorsementSourceTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The business source of the title endorsement form, this can be a state, association or other form provider.</xsd:documentation>
			<xsd:documentation>term: Title Endorsement Source Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ALTA"/>
			<xsd:enumeration value="CLTA"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="TLTA"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TitleOwnershipTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies how ownership for the title policy will be held.</xsd:documentation>
			<xsd:documentation>term: Title Ownership Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Combination">
				<xsd:annotation>
					<xsd:documentation>Ownership will be held by a combination of Individuals, Corporations, Partnerships and Trusts.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Corporation">
				<xsd:annotation>
					<xsd:documentation>Ownership will be held by a corporation.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Estate"/>
			<xsd:enumeration value="GovernmentEntity"/>
			<xsd:enumeration value="Guardianship"/>
			<xsd:enumeration value="Individual">
				<xsd:annotation>
					<xsd:documentation>Ownership will be held by one individual.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="JointVenture"/>
			<xsd:enumeration value="LimitedLiabilityCompany"/>
			<xsd:enumeration value="LimitedPartnership"/>
			<xsd:enumeration value="NonProfitCorporation"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Partnership">
				<xsd:annotation>
					<xsd:documentation>Ownership will be held by a partnership.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Trust">
				<xsd:annotation>
					<xsd:documentation>Ownership will be held by a trust.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TitleProcessInsuranceTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the level of insurance being requested or delivered for the Title Process Type.</xsd:documentation>
			<xsd:documentation>term: Title Process Insurance Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Insurance"/>
			<xsd:enumeration value="LimitedInsurance"/>
			<xsd:enumeration value="NonInsurance"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TitleProcessTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the means used to determine that there are no legal claims against the subject property that have a higher priority than the loan.</xsd:documentation>
			<xsd:documentation>term: Title Process Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DeedReport"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="TitleSearch"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TitleRequestActionTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Describes the action requested for a particular title order.</xsd:documentation>
			<xsd:documentation>term: Title Request Action Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Cancellation">
				<xsd:annotation>
					<xsd:documentation>Indicates that this order is to be cancelled.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Change">
				<xsd:annotation>
					<xsd:documentation>Indicates a change to a previously placed order.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Original">
				<xsd:annotation>
					<xsd:documentation>Indicates that this is the original order.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PriceQuote">
				<xsd:annotation>
					<xsd:documentation>Indicates that this request is merely for a price for a particular title policy.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="StatusQuery">
				<xsd:annotation>
					<xsd:documentation>Indicates that this is a request for status on a previously placed title order.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Update">
				<xsd:annotation>
					<xsd:documentation>Indicates that this request will update a previously placed title order.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TransferOfServicingDisclosureTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies detail about the party servicing the loan as stated on the Servicing Disclosure.</xsd:documentation>
			<xsd:documentation>Specifies the type of servicer associated with the mortgage loan.</xsd:documentation>
			<xsd:documentation>term: Transfer Of Servicing Disclosure Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Investor"/>
			<xsd:enumeration value="Lender"/>
			<xsd:enumeration value="New"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Present"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TrustClassificationTypeEnumeration">
		<xsd:annotation>
			<xsd:documentation>Defines the specific category or type of trust</xsd:documentation>
			<xsd:documentation>term: Trust Classification Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="IllinoisLandTrust"/>
			<xsd:enumeration value="LivingTrust"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TrusteeTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the type of trustee associated with the mortgage loan.</xsd:documentation>
			<xsd:documentation>term: Trustee Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Primary"/>
			<xsd:enumeration value="Secondary"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UPBChangeFrequencyTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Frequency of changes to the unpaid principal balance.</xsd:documentation>
			<xsd:documentation>term: UPB Change Frequency Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Annually"/>
			<xsd:enumeration value="Biweekly"/>
			<xsd:enumeration value="Daily"/>
			<xsd:enumeration value="Monthly"/>
			<xsd:enumeration value="Quarterly"/>
			<xsd:enumeration value="Semiannually"/>
			<xsd:enumeration value="Semimonthly"/>
			<xsd:enumeration value="Weekly"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UniqueDwellingTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>A dwelling that does not conform to a conventional building structure type or method.</xsd:documentation>
			<xsd:documentation>term: Unique Dwelling Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="EarthShelterHome">
				<xsd:annotation>
					<xsd:documentation>A dwelling built with mounds of dirt (earth) covering one or more exterior walls and/or roof.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="GeodesicDome">
				<xsd:annotation>
					<xsd:documentation>A spherical structure designed as a dwelling unit.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Houseboat">
				<xsd:annotation>
					<xsd:documentation>A boat that has been designed or modified to be used primarily as a dwelling unit.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LogHome">
				<xsd:annotation>
					<xsd:documentation>A dwelling made from logs that have not been milled in to conventional lumber.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UnplattedLandTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The type of land description system used for unplatted lands, either Government Survey, or metes and bounds, or other (e.g. land grants).</xsd:documentation>
			<xsd:documentation>term: Unplatted Land Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="GovernmentSurvey"/>
			<xsd:enumeration value="LandGrant"/>
			<xsd:enumeration value="MetesAndBounds"/>
			<xsd:enumeration value="NativeAmericanLand"/>
			<xsd:enumeration value="Ranchero"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VABorrowerCertificationOccupancyTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>VA Borrower Certification type of occupancy. VA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 25(2)</xsd:documentation>
			<xsd:documentation>term: VA Borrower Certification Occupancy Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="A">
				<xsd:annotation>
					<xsd:documentation>I now actually occupy the above described property as my home or intend to move into and occupy said property as my home within a reasonable period of time or intend to reoccupy it after the completion of major alterations, repairs or improvements</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="B">
				<xsd:annotation>
					<xsd:documentation>My spouse is on active military duty and in his or her absence, I occupy or intend to occupy the property securing this loan as my home</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="C">
				<xsd:annotation>
					<xsd:documentation>I previously occupied the property securing this loan as my home. (for interest rate reductions.)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="D">
				<xsd:annotation>
					<xsd:documentation>While my spouse was on active military duty and unable to occupy the property securing this loan, I previously occupied the property that is securing this loan as my home. (for interest rate reduction loans)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VAFundingFeeExemptionTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The Exemption Status given by the VA Administration  (exemption from VA Funding Fees).</xsd:documentation>
			<xsd:appinfo>  SEC  Enumerated   SEC  VA Funding Fee Exemption Type </xsd:appinfo>
			<xsd:documentation>term: VA Funding Fee Exemption Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CompletelyExempt"/>
			<xsd:enumeration value="NotExempt"/>
			<xsd:enumeration value="PartiallyExempt">
				<xsd:annotation>
					<xsd:documentation>The VA will give a percentage of exemption for Veterans that do have a disability, but the disability is not service related.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VerificationMethodTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Method used to verify the particular item.</xsd:documentation>
			<xsd:documentation>term: Verification Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Verbal"/>
			<xsd:enumeration value="Written"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VerificationRangeTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the period or range of time for which the specific type of Documentation Type is collected.</xsd:documentation>
			<xsd:documentation>term: Verification Range Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="MostRecentYear"/>
			<xsd:enumeration value="MostRecentDays"/>
			<xsd:enumeration value="MostRecentMonths"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PaymentPeriod"/>
			<xsd:enumeration value="StatementPeriod"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VerificationStatusTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>This attribute indicates the status of the verification - verified, not verified, or to be verified.</xsd:documentation>
			<xsd:documentation>term: Verification Status Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NotVerified"/>
			<xsd:enumeration value="ToBeVerified"/>
			<xsd:enumeration value="Verified"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VolumeTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Volume type of the document referenced  in the subject document&#13;
</xsd:documentation>
			<xsd:documentation>term: Volume Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Plat">
				<xsd:annotation>
					<xsd:documentation>The volume referenced contains plats</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Deed">
				<xsd:annotation>
					<xsd:documentation>The volume referenced contains deeds</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Mortgage">
				<xsd:annotation>
					<xsd:documentation>The volume referenced contains mortgages</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Maps">
				<xsd:annotation>
					<xsd:documentation>The volume referenced contains various maps</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>The volume referenced contains something other than the enumerated list</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>