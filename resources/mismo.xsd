					<xsd:documentation>Formerly ESCROW_ACCOUNT_ACTIVITY in V2.x.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
