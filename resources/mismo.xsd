				<xsd:annotation>
					<xsd:documentation>Indicates fee is to be included in APR calculations.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentIncludedInStateHighCostIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When true indicates this fee payment amount should be included in state high-cost determination.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentNetDueAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The portion or remaining amount of the fee payment still due from the Borrower or Seller, after application of Paid Out Of Closing and Paid Through Closing fee payment amounts. The resulting fee amount reflected to be still due as required on the HUD-1. CALCULATED RESULT FIELD FORM SPECIFIC FIELD</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentPaidByType" type="RESPA_FeePaymentPaidByTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Party responsible for payment of fee.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentPaidByTypeThirdPartyName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name or description of the party paying the fee when Third Party is the selected enumerated value for RESPA Fee Payment Paid By Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentPaidOutsideOfClosingIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates payment of the RESPA fee was paid outside of closing. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentPercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The portion of the RESPA Fee Total Percent that is to be paid by the specified party. The party is identified in the RESPA Fee Payment Paid By Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentProcessType" type="RESPA_FeePaymentProcessTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Processing - typical buyer paid fee to be disclosed on GFE Closing - typical seller fee not disclosed on GFE</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentRefundableAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount of the fee that is refundable.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentRefundableDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A description the conditions under which the fee is refundable.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentRefundableIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>If true, the fee may be refundable under certain conditions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RESPAFeePaymentSection32Indicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether fee should be included in Section 32 determination</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="RESPA_FEE_PAYMENTS">
		<xsd:sequence>
			<xsd:element name="RESPA_FEE_PAYMENT" type="RESPA_FEE_PAYMENT" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
