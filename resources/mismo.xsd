	<xsd:complexType name="SALES_HISTORIES">
		<xsd:sequence>
			<xsd:element name="SALES_HISTORY" type="SALES_HISTORY" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="SALES_HISTORY">
		<xsd:sequence>
			<xsd:element name="LoanTermDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field describing the term of the loan</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PricePerSquareFootAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The price in terms of dollars per square foot. This is a ratio between a price and an area, but it is a monetary value rather than a factor or rate. (i.e. sales price per gross living area of a property)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PropertySalesAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The sales price of the referenced property.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PropertySalesDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The sale agreement date of a pending or past transfer of the property.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalesConcessionAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of the value of sales concessions granted by an interested party including such items as furniture, carpeting, decorator allowances, automobiles, vacations, securities, giveaways, or other sales incentives.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalesConcessionDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to describe sales concessions granted by an interested party.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalesHistoryAnalysisOfAgreementDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used for the analysis of the terms and conditions of the property sales transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalesHistoryParcelSplitDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Describes the type of parcel split of the sale of the property.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalesHistoryPendingRecordIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the property has "pending records": information which is not yet on the assessor record.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalesHistoryStampAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the stamp amount that was paid to record the document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalesHistoryTransferredOwnershipPercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The percentage of interest/ownership of a property that was transferred. For instance, a person had co-owned (50% ownership) a house with a friend, and sold their share of ownership.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalesHistoryType" type="SalesHistoryTypeEnumerated" minOccurs="0">
