	<xsd:complexType name="FLOOD_REQUEST_DETAIL">
		<xsd:sequence>
			<xsd:element name="BorrowerFloodAcknowledgementLetterIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if a Borrower Acknowledgement Letter needs to be processed with the order.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FEMAAdditionalLenderDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Additional information for the lender in Box 1 of the FEMA form. FORM SPECIFIC FIELD</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodCertificationIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The certification number assigned by the compliance company when an order is received from the submitting / requesting party.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodInsuranceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The minimal Federal requirement for flood insurance coverage, the lesser of: the outstanding principal loan balance; the value of the improved property, mobile home and/or personal property used to secure the loan; or the maximum statutory limit of flood insurance coverage.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodRequestActionType" type="FloodRequestActionTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Describes the type of action to be performed for the request in question.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodRequestCommentText" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Comments for the flood request in question.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodRequestRushIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that this order has a high priority in need for completion.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodTransactionIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A unique tracking identifier assigned by the sender which is typically returned to the sender in the resulting response.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NewServicerAccountIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The vendor account code assigned to the Lender who receives the transfer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OrderingSystemName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the system that originated the order.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OrderingSystemVersionIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The version of the system that originated the order.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OriginalFloodDeterminationLoanIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The loan number assigned to the original flood determination order. Used for identifying a loan that has been previously certified (e.g., refi, second, servicing transfer, etc.)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="FLOOD_REQUEST_DISPUTE">
		<xsd:sequence>
			<xsd:element name="FloodRequestDisputeDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of the reason for the flood dispute.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodRequestDisputeItemType" type="FloodRequestDisputeItemTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the field on the FEMA form which is being disputed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodRequestDisputeItemTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to describe the Other option in the flood request dispute item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodRequestDisputeSupportingDocumentsDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A description of the document available that supports the flood dispute.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="FLOOD_RESPONSE">
		<xsd:sequence>
			<xsd:element name="FLOOD_DETERMINATION" type="FLOOD_DETERMINATION" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Information specific to the Flood Zone Determination pursuant to the National Flood Insurance Program.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FLOOD_RESPONSE_DETAIL" type="FLOOD_RESPONSE_DETAIL" minOccurs="0"/>
			<xsd:element name="LOANS" type="LOANS" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Information specific to a mortgage loan agreement between one or more borrowers and a mortgage lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PARTIES" type="PARTIES" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A collection of objects that define specific parties to a deal.  This includes the direct participating parties, such as borrower and seller and the indirect parties such as the credit report provifer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PROPERTY" type="PROPERTY" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Information specific to the property acting as collateral for the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="FLOOD_RESPONSE_DETAIL">
		<xsd:sequence>
			<xsd:element name="FEMAAdditionalLenderDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Additional information for the lender in Box 1 of the FEMA form. FORM SPECIFIC FIELD</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DisputeResolutionIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the response is a result of a dispute.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodTransactionIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A unique tracking identifier assigned by the sender which is typically returned to the sender in the resulting response.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="FLOOD_RISK_FACTOR">
		<xsd:sequence>
			<xsd:element name="BaseFloodElevationFeetNumber" type="MISMONumeric" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The elevation of flood water in relation to sea level at its highest point for a property. Measured in feet.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FloodDepthFeetNumber" type="MISMONumeric" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The depth of the flood water in relation to the ground for a particular property.  Measured in feet.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PropertyElevationFeetNumber" type="MISMONumeric" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The elevation, in feet, of the property in relation to sea level</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecialFloodHazardAreaDistanceFeetNumber" type="MISMONumeric" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The distance in feet of the nearest Special Flood Hazard Area to a particular property</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="FOREIGN_OBJECT">
		<xsd:annotation>
			<xsd:documentation>Used to hold content that is not part of the namespace or version of the MISMO V3 model  It is either contained directly (via EmbeddedContentXML) or indirectly (via LocationURI). A FOREIGN_OBJECT can be Encoded in a variety of manners without changing its underlying meaning. However, the Encoding of EmbeddedContentXML MUST result in a schema-valid representation. The representation and Encoding are the only transformations supported by the model.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice minOccurs="0">
				<xsd:element name="REFERENCE" type="REFERENCE" minOccurs="0"/>
				<xsd:sequence minOccurs="0">
					<xsd:choice minOccurs="0">
						<xsd:element name="ObjectURI" type="MISMOURI" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The external URI from which the object content can be retrieved.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="EmbeddedContentXML" type="MISMOXML" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation> The XML compatible byte stream in the encoding of the EncodingType that when decoded is a resource of the MediaType using characters from the CharacterEncodingSetType. If a binary EncodingType is not used (e.g. "None" or "EscapedXML") then the CharacterEncodingSetType must be the same as that used for the XML instance document. </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
					<xsd:element name="CharacterEncodingSetType" type="CharacterEncodingSetTypeEnumerated" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Specifies the character encoding set used by the resource content.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CharacterEncodingSetTypeOtherDescription" type="MISMOString" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A free-form text field used to capture the resource Character Encoding Set type if Other is selected for CharacterEncodingSetType.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="MIMETypeIdentifier" type="MISMOIdentifier" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates the Multipurpose Internet Mail Extensions type of the data in the resource. A registered list of these types is available at http://www.iana.org/assignments/media-types/ AND http://tools.ietf.org/html/rfc2045.  In addition, there is a defined syntax for custom MIME types that should be followed when the type is not registered with IANA.</xsd:documentation>
							<xsd:appinfo>Renamed to fix class word.  Replaces "MIME Type".</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ObjectCreatedDatetime" type="MISMODatetime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This attribute may be used to identify the date and time that the resource was created.</xsd:documentation>
							<xsd:appinfo>Renamed.  Used to be "Embedded File Created Date Time".</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ObjectDescription" type="MISMOString" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Provides additional information about the resource, such as the software version that generated the resource.</xsd:documentation>
							<xsd:appinfo>Renamed.  Used to be "Embedded File Description".</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ObjectEncodingType" type="ObjectEncodingTypeEnumerated" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Specifies the type of encoding used to create the object.</xsd:documentation>
							<xsd:appinfo>Renamed.  Used by "Embedded File Encoding Type".</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ObjectEncodingTypeOtherDescription" type="MISMOString" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A free-form text field used to capture the object encoding when Other is selected for Object Encoding Type.</xsd:documentation>
							<xsd:appinfo>Renamed.  Used to be "Embedded File Encoding Type Other Description".</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="OriginalCreatorDigestValue" type="MISMOValue" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Optional placeholder for storing the original digest of the resource at the time of its creation.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ObjectName" type="MISMOString" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifies the resource name (e.g. CreditReport0001.pdf).</xsd:documentation>
							<xsd:appinfo>Renamed.  Used to be "Embedded File Name".</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
				</xsd:sequence>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="FOREIGN_OBJECTS">
		<xsd:annotation>
			<xsd:documentation>A collection of FORREIGN_OBJECT elements</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FOREIGN_OBJECT" type="FOREIGN_OBJECT" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Used to hold content that is not part of the namespace or version of the MISMO V3 model  It is either contained directly (via EmbeddedContentXML) or indirectly (via LocationURI). A FOREIGN_OBJECT can be Encoded in a variety of manners without changing its underlying meaning. However, the Encoding of EmbeddedContentXML MUST result in a schema-valid representation. The representation and Encoding are the only transformations supported by the model.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="FORM_SPECIFIC_CONTENT">
		<xsd:sequence>
			<xsd:choice minOccurs="0">
				<xsd:element name="ASSIGNMENT" type="ASSIGNMENT" minOccurs="0"/>
				<xsd:element name="EXECUTION" type="EXECUTION" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Information regarding the signing event.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="GFE" type="GFE" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Form specific information about the Good Faith Estimate (GFE)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="HUD1" type="HUD1" minOccurs="0">
					<xsd:annotation>
						<xsd:appinfo>Replaces RESPA_HUD_DETAIL container from V2</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="NOTE" type="NOTE" minOccurs="0"/>
				<xsd:element name="RECORDING_ENDORSEMENTS" type="RECORDING_ENDORSEMENTS" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Accommodates the need to capture all endorsements associated with a particular document.  A document may contain reference to a number of previously recorded documents or it may need to be recorded in multiple jurisdictions. </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="RIGHT_TO_CANCEL" type="RIGHT_TO_CANCEL" minOccurs="0"/>
				<xsd:element name="SECURITY_INSTRUMENT" type="SECURITY_INSTRUMENT" minOccurs="0"/>
				<xsd:element name="URLA" type="URLA" minOccurs="0">
					<xsd:annotation>
						<xsd:appinfo>Replaces URLA_TOTAL from Version 2.x</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="DOCUMENT_CLASSIFICATION" type="DOCUMENT_CLASSIFICATION" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Contains information about a specific instance of a document that describes its use (as opposed to its contents).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="FORM_SPECIFIC_CONTENTS">
		<xsd:annotation>
			<xsd:documentation>Data that is not used for anything else except for document preparaation AND does not fit well anywhere else under DEAL.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FORM_SPECIFIC_CONTENT" type="FORM_SPECIFIC_CONTENT" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="FRAUD">
		<xsd:sequence>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="FULFILLMENT_PARTY">
		<xsd:sequence>
			<xsd:element name="FulfillmentPartyRoleType" type="FulfillmentPartyRoleTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This describes the fulfillment partys profession.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FulfillmentPartyRoleTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the fulfillment party role if Other is selected as the Fulfillment Party Role Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FulfillmentPartyServiceType" type="FulfillmentPartyServiceTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Defines the type of service provided by the fulfillment party.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FulfillmentPartyServiceTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the fulfillment party type if Other is selected as the Fulfillment Party Service Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="GENERAL_IDENTIFIER">
		<xsd:sequence>
			<xsd:element name="CongressionalDistrictIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> A two-digit numeric code to identify each Congressional District within a State. These codes are unique within each State. A congressional district identifies an electoral constituency that elects a single member of a congress.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CoreBasedStatisticalAreaCode" type="MISMOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A five digit code representing a functional region based around an urban center of at least 10,000 people, based on standards published by the Office of Management and Budget (OMB) in 2000. The CBSA code refers collectively to metropolitan and micropolitan statistical areas.&#13;
</xsd:documentation>
					<xsd:appinfo>Propose CBSA as a MISMO acronym</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CoreBasedStatisticalAreaDivisionCode" type="MISMOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Metropolitan Divisions are defined within Metropolitan Statistical Areas that have a single core with a population of at least 2.5 million. Not all Metropolitan Statistical Areas with urbanized areas of this size will contain Metropolitan Divisions. Metropolitan Division codes are distinguished by a five-digit code ending in 4.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JudicialDistrictName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the Judicial District in which the subject property resides.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JudicialDivisionName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the Judicial Division in which the subject property resides.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MapReferenceIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A reference to a regionally specific map document that assists in locating a property. May refer to locally available published map products (e.g. Thomas Map in CA) or a county tax map.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MapReferenceSecondIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A secondary reference to a regionally specific map document that assists in locating a property. May refer to locally available published map products (e.g. Thomas Map in CA) or a county tax map.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MSAIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the Metropolitan Statistical Area (MSA) where subject property is located. An MSA is a contiguous geographic area consisting of an urban center city and its surrounding suburbs. MSAs are defined by the federal Office of Management and Budget.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MunicipalityName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the municipality in which property is located.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecordingJurisdictionName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the Recording Jurisdiction in which the subject property resides.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecordingJurisdictionType" type="RecordingJurisdictionTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of Recording Jurisdiction in which the subject property is located.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecordingJurisdictionTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The description of the Recording Jurisdiction Type when Other is selected from the enumerated list.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SchoolDistrictName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the school district in which the property is located.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="GEOSPATIAL_INFORMATION">
		<xsd:sequence>
			<xsd:element name="LatitudeIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Gives the location of a place on Earth  north or south of the equator. Lines of Latitude are the horizontal lines shown running east-to-west on maps (particularly so in the Mercator projection). Technically, latitude is an angular measurement in degrees ranging from 0 degrees at the equator (low latitude) to 90 degree at the poles (90 degree N or +90 degree for the North Pole and 90 degree S or −90 degree for the South Pole). </xsd:documentation>
					<xsd:documentation>MAL: 10/12 - May be replaced by OpenGeoSpacial.org GPS standard&#13;
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LongitudeIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The geographic coordinate most commonly used in cartography and global navigation for east-west measurement. It is the angular distance measured east or west and usually expressed in degrees (or hours), minutes, and seconds, from the prime meridian, defined to be at the Royal Observatory, Greenwich, in England, to the meridian passing through another position on the earth's surface. </xsd:documentation>
					<xsd:documentation>MAL: 10/12 - May be replaced by OpenGeoSpacial.org GPS standard</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="GFE">
		<xsd:annotation>
			<xsd:documentation>Form specific information about the Good Faith Estimate (GFE)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="GOVERNMENT_BORROWER">
		<xsd:annotation>
			<xsd:documentation>All data points that are unique to processing a government loan including FHA, VA, Rural Housing, etc. This contains the contents of the combined FHA_BORROWER, FHA_VA_BORROWER and VA_BORROWER containers from Version 2.</xsd:documentation>
			<xsd:documentation>Created new container which houses previous 3 containers: FHA Borrower, VA Borrower, and FHA_VA Borrower</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CAIVRSIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An identifier assigned by HUDs Credit Alert Interactive Voice Response System, an interactive system used to check the applicants outstanding federal obligations.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationLeadPaintIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Property built prior to 1978, Lead Paint Notice provided to borrower. FHA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 25(6)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationOriginalMortgageAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Original mortgage amount of property that was sold with in the past 60 months secured by HUD/FHA mortgage. FHA Borrower Certification HUD92900-A (FHA/VA Addendum to URLA) value used in Section 22c: Original Mortgage Amount.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationOwnOtherPropertyIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Do you own or have you sold other real estate within the past 60 months on which there was a HUD/FHA mortgage? FHA Borrower Certification, HUD-92900-A, (HUD/VA Addendum to URLA) Section 22a:</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationOwn4OrMoreDwellingsIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Does borrower own four or more dwellings. FHA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 22f.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationPropertySoldCityName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>City of property sold in the past 60 months on which there was a HUD/FHA mortgage. FHA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 22d</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationPropertySoldPostalCode" type="MISMOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Postal Code of property sold in the past 60 months on which there was a HUD/FHA mortgage. FHA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 22d</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationPropertySoldStateName" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>State of property sold in the past 60 months on which there was a HUD/FHA mortgage. FHA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 22d</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationPropertySoldStreetAddressLineText" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Street address of property sold in the past 60 months on which there was a HUD/FHA mortgage. FHA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 22d</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationPropertyToBeSoldIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>If owned property within the past 60 months that was secured by a HUD/FHA mortgage, is it to be sold? FHA Borrower Certification, HUD-92900-A, (HUD/VA Addendum to URLA) Section 22a:</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationRentalIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Property covered by mortgage is to be rented, or is adjacent to rental properties involving eight or more dwelling units in which the borrower has a financial interest. FHA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 22e</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHABorrowerCertificationSalesPriceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Sales price of property sold in the past 60 months that was secured by HUD/FHA mortgage. FHA Borrower Certification, HUD-92900-A, (HUD/VA Addendum to URLA) Section 22b:</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHA_VABorrowerCertificationSalesPriceExceedsAppraisedValueType" type="FHA_VA_BorrowerCertificationSalesPriceExceedsAppraisedValueTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>FHA/VA Borrower acknowledgement when Sales Price exceeds the Appraised Value. FHA/VA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 25(3)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VABorrowerCertificationOccupancyType" type="VABorrowerCertificationOccupancyTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>VA Borrower Certification type of occupancy. VA Borrower Certification, HUD-92900-A (HUD/VA Addendum to URLA), section 25(2)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VABorrowerSurvivingSpouseIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>VA Borrower Surviving Spouse Indicator</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VACoBorrowerNonTaxableIncomeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of the co-borrower's monthly income that is not subject to taxation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VACoBorrowerTaxableIncomeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of the co-borrower's monthly income that is subject to taxation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAFederalTaxAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The monthly amount of federal income tax from the applicable table for VA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VALocalTaxAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The monthly amount of income tax other than that paid to state or federal governments from the applicable table for VA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAPrimaryBorrowerNonTaxableIncomeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of the primary borrower's monthly income that is not subject to taxation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAPrimaryBorrowerTaxableIncomeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of the borrower's monthly income that is subject to taxation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VASocialSecurityTaxAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The monthly amount of social security tax from the applicable table for VA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAStateTaxAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The monthly amount of state income tax from the applicable table for VA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VeteranStatusIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether the borrower qualifies as a Veteran under HUD guidelines and that the loan application should be evaluated as a 203(b)(2) Veteran Loan. If any borrower qualifies as a Veteran under VA guidelines then the loan (including all borrowers) may be processed as a VA loan application.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="GOVERNMENT_LOAN">
		<xsd:sequence>
			<xsd:element name="AgencyProgramDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Describes the federal, state, or local agency program under which the loan is originated.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerFinancedFHADiscountPointsAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of discount points allowable by the applicable FHA guidelines to be financed on a FHA no cash out refinance. Used for FHA loans. Note: Discount points financed and not financed by borrower are equal to the Discount Points Total Amount Paid by Borrower - FHA.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerFundingFeePercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The funding fee percentage due from the borrower. The fee is entered as a percentage.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerHomeInspectionChosenIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When true indicates that borrower(s) have chosen to have a home inspection performed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerPaidFHA_VAClosingCostsAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The estimated dollar amount of closing costs that will be paid by the borrower on the subject property excluding discount points and prepaid items. Used for FHA and VA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BorrowerPaidFHA_VAClosingCostsPercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The percentage of the estimated closing costs that will be paid by the borrower on the subject property excluding discount points and prepaids.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DaysToFHA_MIEligibilityCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the number of days in which the borrower will be determined to be eligible for insurance under the National Housing Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHA_MIPremiumRefundAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>For refinance loans, the amount of the MIP that will be refunded to the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHA_VALoanOriginatorIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The FHA or VA assigned number of the originator of the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHAAlimonyLiabilityTreatmentType" type="FHAAlimonyLiabilityTreatmentTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies how the borrower or lender prefers that the judgment process treat alimony in an FHA transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHAAssignmentDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date the lender registers the loan through FHA connection. (This date can impact the upfront and renewal MI rates for FHA Loans.)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHACoverageRenewalRatePercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the percentage of the renewal mortgage insurance premiums for FHA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHALoanLenderIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An FHA-assigned identifier of the originator making an underwriting request using the FHA Total Scorecard program.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHALoanSponsorIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An FHA-assigned identifier of the lender acting as an approved sponsor for the originator making the underwriting request using the FHA Total Scorecard program.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHAEnergyRelatedRepairsOrImprovementsAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Dollar amount considered for energy related items.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHAGeneralServicesAdministrationIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The General Services Administrations (GSA) identifier from the List of Parties Excluded from Federal Procurement or Nonprocurement Programs. This is documented on the HUD-92900-WS.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHALimitedDenialParticipationIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifier obtained from review of HUDs Limited Denial of Participation (LDP). This identifier is placed on the HUD-92900-WS.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHANonOwnerOccupancyRiderRule248Indicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>On the FHA Non Owner Occupancy Rider, this indicates that the Borrower is an Indian Tribe as provided in Section 248 of the National Housing Act or a serviceperson who is unable to occupy the Property because of his or her duty assignment as provided in Section 216 or Subsection(b)(4) or (f) of Section 222 of the National Housing Act.  </xsd:documentation>
					<xsd:documentation>FORM SPECIFIC FIELD.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHARefinanceInterestOnExistingLienAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Dollar amount of interest on existing lien, for FHA refinance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHARefinanceOriginalExistingFHACaseIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Original FHA Case Number for refinance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHARefinanceOriginalExistingUpFrontMIPAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Amount paid for Original Upfront MIP for refinance loan</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHAUnderwriterComputerizedHomesUnderwritingSystemIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The FHA assigned number that identifies the Direct Endorsement underwriter authorized to approve the loan on behalf of the FHA.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FHAUpfrontMIPremiumPercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The percentage of upfront MIP, used for FHA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GovernmentMortgageCreditCertificateAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of the Mortgage Credit Certificate. This is entered as a whole number. It is an annual tax credit. FHA allows it to be treated as monthly income. May also be used for VA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GovernmentRefinanceType" type="GovernmentRefinanceTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the amount of documentation for FHA/VA no cash out refinance loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GovernmentRefinanceTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for Government Refinance Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HUDAdequateAvailableAssetsIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that borrower has adequate available assets for HUD approval</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HUDAdequateEffectiveIncomeIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates borrower effective income is adequate for HUD approval</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HUDCreditCharacteristicsIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates borrower meets credit characteristics for HUD approval</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HUDStableEffectiveIncomeIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates borrowers stable effective income meets HUD approval</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherPartyPaidFHA_VAClosingCostsAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The estimated closing costs which will be paid by a party, other than the borrower or seller, on the subject property. Used for FHA or VA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherPartyPaidFHA_VAClosingCostsPercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The percentage of estimated closing costs that will be paid by any party other than the borrower or seller including any seller paid prepaids but excluding discount points.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PropertyEnergyEfficientHomeIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the appraiser states that the property meets FHA /VA Energy Efficient Home guidelines. Usually applies to new construction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RuralHousingConditionalGuarantyExpirationDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>For Rural Housing loans, the expiration date of the guaranty.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RuralHousingConditionalGuarantyInterestRatePercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>For Rural Housing loans, the interest rate included in the USDA guaranty.</xsd:documentation>
					<xsd:appinfo>Renamed.  Used to be "Rural Housing Conditional Guaranty Interest Rate".</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SectionOfActType" type="SectionOfActTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the section of the National Housing Act that defines underwriting guidelines for VA or FHA loan evaluations.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SectionOfActTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for Section of Act Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SellerPaidFHA_VAClosingCostsPercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The percentage of estimated closing costs that will be paid by the seller of the subject property, including any seller prepaids, but excluding discount points.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SoldUnderHUDSingleFamilyPropertyDispositionProgramIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>On the FHA Non Owner Occupancy Rider, this indicates that the Security Instrument applies to Property sold under HUD Single Family Property Disposition Program and meets the requirements thereof. FORM SPECIFIC FIELD.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="USDA198021RuralHousingAdjustedAnnualHouseholdIncomeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The total annual income of all members of the household as defined by Rural Housing Program Guidelines.  This amount can be found on USDA Form 1980-21, Line 7. FORM SPECIFIC FIELD</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VABorrowerCoBorrowerMarriedIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether the co-borrower is married to the borrower. Used for VA loans only.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAEntitlementAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of guarantee by the VA available to the veteran borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAEntitlementIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>VA entitlement identifier from Certificate of Eligibility for VA Loan Summary Sheet.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAFundingFeeExemptionType" type="VAFundingFeeExemptionTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Exemption Status given by the VA Administration  (exemption from VA Funding Fees).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAFundingFeeExemptionTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free form text field used to collect additional information when Other is selected for VA  Funding Fee Exemption Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAHouseholdSizeCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Size of household at the loan level as defined by the VA. The lender must meet VA requirements for determining the size of household.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAMaintenanceExpenseMonthlyAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The estimated dollar amount of monthly maintenance expense used for VA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAResidualIncomeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of the monthly balance of funds available for family support after meeting the borrowers shelter expenses, debts, and taxes as defined by the VA.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VAUtilityExpenseMonthlyAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The estimated dollar amount of monthly utility expense used for VA loans.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="GOVERNMENT_MONITORING">
		<xsd:annotation>
			<xsd:documentation>Information about the HMDA data that is collected about the borrower including the borrower's race.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="GOVERNMENT_MONITORING_DETAIL" type="GOVERNMENT_MONITORING_DETAIL" minOccurs="0">
				<xsd:annotation>
					<xsd:appinfo>Additional information about the Government Monitoring.&#13;
</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HMDA_RACES" type="HMDA_RACES" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="GOVERNMENT_MONITORING_DETAIL">
		<xsd:annotation>
			<xsd:appinfo>Additional information about the Government Monitoring.&#13;
</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="GenderType" type="GenderTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The borrower’s or interviewer’s statement about the borrower's gender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HMDARefusalIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the borrower has refused to provide the requested HMDA information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HMDAEthnicityType" type="HMDAEthnicityTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The borrower’s or interviewer’s statement about the borrower’s ethnicity as defined in the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RaceNationalOriginRefusalIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Borrowers answer to the statement: I do not wish to furnish the HMDA) information. Collected on the URLA in Section X.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="HAZARD_INSURANCE">
		<xsd:annotation>
			<xsd:documentation>Information specific to any hazard insurance policy on the property that secures the loan. There may be multiple policies, such as homeowners, fire and extended coverage, flood, wind, or earthquake insurance.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FloodContractFeeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount charged by the provider to obtain the Flood Contract associated with the subject property.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceCoverageAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The coverage amount of the property casualty policy on the property securing the loan</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceCoverageType" type="HazardInsuranceCoverageTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of loss the insurance policy covers.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceCoverageTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the hazard insurance coverage type if Other is selected as the hazard insurance coverage type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsurancePremiumAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The projected property casualty premium to be paid.</xsd:documentation>
					<xsd:appinfo>Renamed.  Was "Hazard Insurance Disbursement Amount"</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsurancePremiumMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The term expressed in months between each property casualty insurance premium.</xsd:documentation>
					<xsd:appinfo>Renamed.  Was "Hazard Insurance Disbursement Term"&#13;
Need to set new classword.</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceEffectiveDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date the hazard insurance went into effect.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceEscrowedIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether hazard is paid by the borrower or the lender</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceExpirationDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date the property casualty policy expires</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceNextPremiumDueDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The month and year the next property casualty premium is due</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceNonStandardPolicyType" type="HazardInsuranceNonStandardPolicyTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Additional Description of the Hazard Insurance coverage document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceNonStandardPolicyTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field used to capture the Hazard Insurance Non Standard Policy Type if Other is selected as the Hazard Insurance Non Standard Policy Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsurancePolicyCancellationDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date the insurance company terminates the policy.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsurancePolicyIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number assigned to the policy by the insurance company</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HazardInsuranceReplacementValueIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A code that indicates whether the coverage provides full replacement of the property</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
