			<xsd:element name="RESPA_FEE_DETAIL" type="RESPA_FEE_DETAIL" minOccurs="0"/>
			<xsd:element name="RESPA_FEE_PAYMENTS" type="RESPA_FEE_PAYMENTS" minOccurs="0"/>
			<xsd:element name="RESPA_FEE_REQUIRED_SERVICE_PROVIDER" type="RESPA_FEE_REQUIRED_SERVICE_PROVIDER" minOccurs="0"/>
			<xsd:element name="RESPA_FEE_PAID_TO" type="RESPA_FEE_PAID_TO" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
