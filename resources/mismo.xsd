	<xsd:simpleType name="ForeclosureStatusTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Indicates the most recent step in the foreclosure proceedings.</xsd:documentation>
			<xsd:documentation>term: Foreclosure Status Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
