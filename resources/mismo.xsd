			<xsd:enumeration value="Fixed"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HMDAEthnicityTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The borrower’s or interviewer’s statement about the borrower’s ethnicity as defined in the Home Mortgage Disclosure Act.</xsd:documentation>
			<xsd:documentation>The borrowers or interviewers statement about the borrowers ethnicity as defined in the Home Mortgage Disclosure Act (HMDA).</xsd:documentation>
			<xsd:documentation>term: HMDA Ethnicity Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="HispanicOrLatino">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="InformationNotProvidedByApplicantInMailInternetOrTelephoneApplication">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NotApplicable">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act. Used for loans where the borrower is an institution, corporation, or partnership.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NotHispanicOrLatino">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HMDAPurposeOfLoanTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the HMDA Purpose of Loan as defined in Regulation C. The definitions of a home improvement loan and a refinancing were substantially revised in the final rules adopted in 2002.</xsd:documentation>
			<xsd:documentation>term: HMDA Purpose Of Loan Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="HomeImprovement"/>
			<xsd:enumeration value="HomePurchase"/>
			<xsd:enumeration value="Refinancing"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HMDARaceTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The borrower’s or interviewer’s statement about the borrower's race as defined in the Home Mortgage Disclosure Act. Up to 5 Race Types may be included per borrower.</xsd:documentation>
			<xsd:documentation>term: HMDA Race Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AmericanIndianOrAlaskaNative">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Asian">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BlackOrAfricanAmerican">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="InformationNotProvidedByApplicantInMailInternetOrTelephoneApplication">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NativeHawaiianOrOtherPacificIslander">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NotApplicable">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act. Used for loans where the borrower is an institution, corporation, or partnership.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="White">
				<xsd:annotation>
					<xsd:documentation>Category as defined by the Home Mortgage Disclosure Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HUD1LineItemPaidByTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The party responsible for payment of the specified HUD-1 line item.</xsd:documentation>
			<xsd:appinfo>Renamed.  Used to be "RESPA HUD Detail Line Item Paid By Type".</xsd:appinfo>
			<xsd:documentation>term: HUD1 Line Item Paid By Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Buyer"/>
			<xsd:enumeration value="LenderPremium"/>
			<xsd:enumeration value="Seller"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HazardInsuranceCoverageTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The type of loss the insurance policy covers.</xsd:documentation>
			<xsd:documentation>term: Hazard Insurance Coverage Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Earthquake"/>
			<xsd:enumeration value="FireAndExtendedCoverage"/>
			<xsd:enumeration value="Flood"/>
			<xsd:enumeration value="Hazard"/>
			<xsd:enumeration value="Homeowners"/>
			<xsd:enumeration value="Hurricane"/>
			<xsd:enumeration value="InsectInfestation"/>
			<xsd:enumeration value="Leasehold"/>
			<xsd:enumeration value="MineSubsidence"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="PersonalProperty"/>
			<xsd:enumeration value="Storm"/>
			<xsd:enumeration value="Tornado"/>
			<xsd:enumeration value="Volcano"/>
			<xsd:enumeration value="Wind"/>
		</xsd:restriction>
