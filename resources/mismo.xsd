	<xsd:complexType name="ESCROW_ITEM">
		<xsd:sequence>
			<xsd:element name="ESCROW_ITEM_DETAIL" type="ESCROW_ITEM_DETAIL" minOccurs="0"/>
			<xsd:element name="ESCROW_ITEM_DISBURSEMENTS" type="ESCROW_ITEM_DISBURSEMENTS" minOccurs="0"/>
			<xsd:element name="ESCROW_PAID_TO" type="ESCROW_PAID_TO" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ESCROW_ITEMS">
		<xsd:sequence>
			<xsd:element name="ESCROW_ITEM" type="ESCROW_ITEM" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="ESCROW_ITEM_DETAIL">
		<xsd:sequence>
			<xsd:element name="EscrowAnnualPaymentAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount of the annual payment for the Escrow Item Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowCollectedNumberOfMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of months escrow to be collected at closing for the Escrow Item Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowInsurancePolicyIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The identifier assigned to the insurance policy by the issuing company.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowItemLastDisbursementDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the last disbursement date for the selected escrow item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowItemNextDisbursementDueDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the due date of the next disbursement for the selected escrow item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowItemNextProjectedDisbursementAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the projected amount of the next disbursement due on this escrow item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowItemType" type="EscrowItemTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specifies the type of Escrow Item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowItemTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A free-form text field to collect additional information when Other is selected for Escrow Item Type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowMonthlyPaymentAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The monthly payment amount for the escrow item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowMonthlyPaymentRoundingType" type="EscrowMonthlyPaymentRoundingTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Defines the rounding method applied to the escrow payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowPaidByType" type="EscrowPaidByTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of Party responsible for payment of the escrow item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowPaymentFrequencyType" type="EscrowPaymentFrequencyTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Payment period covered by escrow item. If item were paid once a year the payment frequency type would be annual.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowPaymentFrequencyTypeOtherDescription" type="MISMOString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When Other is specified as the Payment Frequency Type, this data element contains the description.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowPremiumAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of the escrow premium for the escrow item type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowPremiumDurationMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of months covered by the related premium.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowPremiumPaidByType" type="EscrowPremiumPaidByTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of party responsible for the payment of the escrow premium amount.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowPremiumPaymentType" type="EscrowPremiumPaymentTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The payment method of the premium for the escrow item. Paid Outside of Closing (POC), Waived or Collect at Closing.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowSpecifiedHUD1LineNumberValue" type="MISMONumericString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The line number on the HUD-1 where the Escrow Item is to be disclosed. FORM SPECIFIC FIELD</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EscrowTotalDisbursementAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Total amount disbursed for the escrow item from closing to the current date.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="ESCROW_ITEM_DISBURSEMENT">
