				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="REHABILITATION">
		<xsd:sequence>
			<xsd:element name="RehabilitationLoanFundsAmount" type="MISMOAmount" minOccurs="0">
