			<xsd:enumeration value="365"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HELOCTeaserRateTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>The type of introductory (teaser) rate on a line of credit.</xsd:documentation>
			<xsd:appinfo>  SEC  Enumerated   SEC  HELOC Teaser Rate Type </xsd:appinfo>
			<xsd:documentation>term: HELOC Teaser Rate Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
