	<xsd:simpleType name="DocumentSupertypeTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Allows trading partners to classify documents at a broader level than required by the more granular Types and Subtypes, as appropriate for the business purpose. There is only one SuperType assigned to each type. This can be sent as an alternate to sending Type.</xsd:documentation>
			<xsd:documentation>term: Document Supertype Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BorrowerApplication"/>
			<xsd:enumeration value="BorrowerCreditAndLiabilities"/>
			<xsd:enumeration value="BorrowerIncomeAndAssets"/>
			<xsd:enumeration value="Closing"/>
			<xsd:enumeration value="Disclosure"/>
			<xsd:enumeration value="Note"/>
			<xsd:enumeration value="Property"/>
			<xsd:enumeration value="SecurityInstrument"/>
			<xsd:enumeration value="Title"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DocumentTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies a specific document defined at a granular level.  May be modified or further constrained by other Document data to make the document instance more specific. Further defined in the MISMO Document Classification I-Guide.</xsd:documentation>
			<xsd:documentation>term: Document Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="203KConsultantReport">
				<xsd:annotation>
					<xsd:documentation>Describes the work that needs to be done to bring the house to FHA standards.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="203KHomeownerAcknowledgement">
				<xsd:annotation>
					<xsd:documentation>Confirms the final bid amount for the project and the time frame for completion.  A contract between the homeowner and the contractor.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="203KInitialDrawRequest">
				<xsd:annotation>
					<xsd:documentation>Provides information required to release funds based on work in place. Must be signed by contractor, borrower and inspector.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="203KMaximumMortgageWorksheet">
				<xsd:annotation>
					<xsd:documentation>Determines Maximum amount for 203K Mortgage based on the loan amount; closing costs; sales price (if purchase) and work to be performed. Provided to the Lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="203KRehabilitationAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the terms and conditions of the 203K loan program works and describes the guidelines that will be followed in administering funds.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AbstractNoticeAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes that the lender will not be responsible for replacing the abstract. </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AbstractOfJudgment">
				<xsd:annotation>
					<xsd:documentation>Derived from a Judgment; contains only the details of a court action that relate to fulfilling the court order.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AcknowledgementOfNoticeOfRightToCancel">
				<xsd:annotation>
					<xsd:documentation>Confirms that a borrower has received and understands the Notice of Right to Cancel.  Sometimes contained with the Notice of Right to Cancel.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AffidavitOfDeath">
				<xsd:annotation>
					<xsd:documentation>Establishes the fact of death but does not contain personally identifiable information.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AffiliatedBusinessArrangementDisclosure">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of any Lender-related businesses (i.e., subsidiaries or affiliates) and borrower options related to using these providers. This document is a part of HUD Regulation X (RESPA/Reg X).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AirportNoisePollutionAgreement">
				<xsd:annotation>
					<xsd:documentation>Confirms that a borrower is aware of presence of airport and possible noise situations. Provided by Borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AmendatoryClause">
				<xsd:annotation>
					<xsd:documentation>Adds HUD required conditions to the Purchase Agreement. Escape clause allowing a borrower to exit transaction if it does not meet value.  These do not have HUD number but are required in Purchase Agreement.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AmortizationSchedule">
				<xsd:annotation>
					<xsd:documentation>Describes interest and principal payments to a borrower from a lender, or lender to borrower, and their effect on the balance of the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AppraisalRecertificationForm">
				<xsd:annotation>
					<xsd:documentation>Confirms that the appraised value of a home has not decreased.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AppraisalReport">
				<xsd:annotation>
					<xsd:documentation>Describes findings of the appraisal process and provides an estimation of the subject property value.  Provided by a qualified appraiser.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Appraisal Review">
				<xsd:annotation>
					<xsd:documentation>Documents the results of a independent evaluation of an appraisal </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AppraisalReviewRequest">
				<xsd:annotation>
					<xsd:documentation>Requests an internal review and evaluation of an appraisal. </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ApprovalLetter">
				<xsd:annotation>
					<xsd:documentation>Describes the loan approval status and any associated conditions to the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ArticlesOfIncorporation">
				<xsd:annotation>
					<xsd:documentation>Establishes a corporation's purpose and structure, and percentages of ownership.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Assignment">
				<xsd:annotation>
					<xsd:documentation>Transfers the beneficial interest in a instrument from one party to another.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AssumptionAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes that the buyer will assume responsibility for the obligations of a mortgage from the builder or original owner.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AssuranceOfCompletion">
				<xsd:annotation>
					<xsd:documentation>Describes escrowed repairs and certifies that escrowed repairs will be completed.  Provided by Lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AttorneyInFactAffidavit">
				<xsd:annotation>
					<xsd:documentation>Confirms that a person is under a Power of Attorney and warrants specifics facts about the person such as: POA has not been revoked, principle has not died, no divorce action has been commenced if spouse of the principle, a guardian or conservator has not been appointed for the Principle.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AttorneysOpinionLetter">
				<xsd:annotation>
					<xsd:documentation>Addresses trust or other property matters and confirms the legality of the information that is being provided to the lender.  This could consist of beneficiary information, assets used as collateral, trustee and revocable nature of trusts. In Some States, Title insurance is provided via an Attorney's Opinion  Letter.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AutomatedClearingHouseDebitForm">
				<xsd:annotation>
					<xsd:documentation>Establishes a direct debit from a borrowers account for loan or escrow payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AutomatedUnderwritingFeedback">
				<xsd:annotation>
					<xsd:documentation>Confirms results from Desk Top Underwriter, Loan Prospector or other underwriting system, including the loan decision and conditions or stipulations of the approval.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AutomatedValueModelFeedback">
				<xsd:annotation>
					<xsd:documentation>Confirms results from an automated property valuation system.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BaileeLetter">
				<xsd:annotation>
					<xsd:documentation>Confirms that a company will return to the funding lender the collateral package, the funded value, or mortgage-backed security value of a property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BalanceTransferAuthorizationNotice">
				<xsd:annotation>
					<xsd:documentation>Informs borrower of what will happen when lender is instructed to pay off debt on behalf of a borrower directly from a Home Equity account.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BalloonRefinanceDisclosure">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that rate and payment amount may change when the refinance option is chosen for balloon loans and describes how the interest rate and payments are determined.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BankDepositSlip">
				<xsd:annotation>
					<xsd:documentation>Provides written evidence of a deposit to a financial institution.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BankruptcyDischargeNotice">
				<xsd:annotation>
					<xsd:documentation>Confirms that a person or company is legally free and clear to repay certain debts.  Provided by the court.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BankStatement">
				<xsd:annotation>
					<xsd:documentation>Describes transactions recorded and account balances of a customer's accounts during a specified period.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BenefitPlanDistributionStatement">
				<xsd:annotation>
					<xsd:documentation>Outlines the details for distribution of funds from a benefit plan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Bid">
				<xsd:annotation>
					<xsd:documentation>Describes required home repairs or improvements and provides an estimation of cost to complete.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BirthCertificate">
				<xsd:annotation>
					<xsd:documentation>Confirms details about an individual's birth.  An official record provided by a government entity such as a state, county, city or borough.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BondCertificate">
				<xsd:annotation>
					<xsd:documentation>Confirms the ownership of a bond.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BorrowerAcknowledgmentOfPropertyCondition">
				<xsd:annotation>
					<xsd:documentation>Confirms that the borrowers understand the condition of the property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BorrowerCorrespondence">
				<xsd:annotation>
					<xsd:documentation>Describes items related to the application including borrower credit, income, employment or assets. Typically provided by borrower.  Includes items provided by third parties in support of the borrower's application. Documents provided by the lender to be signed by the borrower are not considered Borrower Correspondence.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BorrowersCertification">
				<xsd:annotation>
					<xsd:documentation>Authorizes lender to request personal information on the borrower's behalf.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BorrowersContractWithRespectToHotelAndTransientUseOfProperty">
				<xsd:annotation>
					<xsd:documentation>Certification that the borrower will not use a multiple dwelling property for hotel or transient housing.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BorrowerLienAffidavit">
				<xsd:annotation>
					<xsd:documentation>Verifies that there are no outstanding liens or mortgages that would not allow for the appropriate lien position of a new loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BrokerDisclosureStatement">
				<xsd:annotation>
					<xsd:documentation>Describes sources and amounts of compensation a brokered mortgage transaction.  Provided by Broker to Borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BrokerPriceOpinion">
				<xsd:annotation>
					<xsd:documentation>Describes findings of the broker review process and provides an estimation of the subject property value.  The report typically combines information from a drive-by exterior examination, and various external data sources, but not an interior examination.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BuildersCertification">
				<xsd:annotation>
					<xsd:documentation>Confirms that the property construction is completed to requirements. Provided by builder.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BuildingPermit">
				<xsd:annotation>
					<xsd:documentation>Authorizes the builder to build on the property. Provided by local municipal authority.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BusinessLicense">
				<xsd:annotation>
					<xsd:documentation>Authorizes the legal operation of a business in an area issued by the state or municipality.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BuydownAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes a rate variant, or temporary adjustment, of interest rate on the loan.  Used to reduce, or step down, an interest rate.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BuyingYourHomeSettlementCostsAndHelpfulInformation">
				<xsd:annotation>
					<xsd:documentation>Describes the settlement process, charges, rights and remedies available under RESPA, as well as explanation of settlement services and costs.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CancellationOfListing">
				<xsd:annotation>
					<xsd:documentation>Confirms that a property is no longer listed with a real estate agent.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Check">
				<xsd:annotation>
					<xsd:documentation>Represents money payable or paid (if cancelled).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Checklist">
				<xsd:annotation>
					<xsd:documentation>Describes the list of action items, steps, or elements required to be consulted or completed to accomplish a task.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ChildSupportVerification">
				<xsd:annotation>
					<xsd:documentation>Describes the legal obligation of a parent to pay money toward the care and maintenance of his/her children.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CloseLineOfCreditRequest">
				<xsd:annotation>
					<xsd:documentation>Requests that a line of credit be closed after receiving a payoff under a separate cover.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ClosingInstructions">
				<xsd:annotation>
					<xsd:documentation>Describes the conditions that must be met in order to complete the closing. Provided by Lender to Closing Agent.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ComplianceAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes that the borrower will accommodate changes to loan documentation that result from errors and omissions, to ensure that the documents conform to industry standards.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ComplianceInspectionReport">
				<xsd:annotation>
					<xsd:documentation>Confirms that property construction, repairs or improvements related to the loan (but not to the appraisal per se) have been completed acceptably.  Provided by appraiser or inspector. This is not Fannie Mae form 442.  Ref HUD form 92051.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ConditionalCommitment">
				<xsd:annotation>
					<xsd:documentation>Establishes the property value and describes repair conditions.  Provided by Underwriter. This is only related to appraisal and property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CondominiumOccupancyCertificate">
				<xsd:annotation>
					<xsd:documentation>Confirms the percentage of a Condominium (Condo) Association or phase is currently occupied.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ConservatorAndGuardianshipAgreement">
				<xsd:annotation>
					<xsd:documentation>Authorizes a person to sign documents for a borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ConsolidationExtensionModificationAgreement">
				<xsd:annotation>
					<xsd:documentation>Changes one or more of the terms of a mortgagor's loan in the security instrument.  Used for refinance loans in state of NY.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ConstructionCostBreakdown">
				<xsd:annotation>
					<xsd:documentation>Describes in detail the construction costs to build a new structure.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ConsumerHandbookOnAdjustableRateMortgages">
				<xsd:annotation>
					<xsd:documentation>Describes Adjustable Rate Mortgages and reviews their benefits and risks.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ConveyanceDeed">
				<xsd:annotation>
					<xsd:documentation>Transfers title to a property from a seller (grantor) to a recipient (grantee); provides evidence of the legal right of ownership.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ConveyanceTaxForm">
				<xsd:annotation>
					<xsd:documentation>Contains data about the property sale and conveyance tax calculations. Accompanies a Conveyance Deed at the time of recording.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CooperativeBylaws">
				<xsd:annotation>
					<xsd:documentation>Establishes the rules for governing and operating the Cooperative (Coop) Housing Corporation.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CooperativeOperatingBudget">
				<xsd:annotation>
					<xsd:documentation>Describes the planned revenues and expenditures of the Cooperative (Coop) for the current fiscal year.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CooperativeProprietaryLease">
				<xsd:annotation>
					<xsd:documentation>Establishes the rights and duties of the Cooperative (Coop) and member shareholder under which the shareholder is allowed to use a certain unit in the premises.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CooperativeRecognitionAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the responsibility of the Cooperative (Coop)  to notify the lender in the event of borrower default.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CooperativeStockCertificate">
				<xsd:annotation>
					<xsd:documentation>Confirms an individual's ownership interest in the Cooperative.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CooperativeStockPower">
				<xsd:annotation>
					<xsd:documentation>Authorizes the lender to assign the borrowers stock certificate in case of default.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CosignerNotice">
				<xsd:annotation>
					<xsd:documentation>Informs a cosigner of a loan that they will be obligated to pay the debt if the main borrower's) default on the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CouncilOfAmericanBuildingOfficialsCertification">
				<xsd:annotation>
					<xsd:documentation>Confirms that the property was constructed to CABO (Council of American Building Officials) standards. Provided by builder.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CounselingCertification">
				<xsd:annotation>
					<xsd:documentation>Confirms that the borrower participated in homeownership counseling.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CounselingChecklistForMilitaryHomebuyers">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of certain Veterans' Affairs items of interest regarding the loan and financing eligibility.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CreditAlertInteractiveVoiceResponseSystem">
				<xsd:annotation>
					<xsd:documentation>Confirms results from the HUD Credit Alert Interactive Voice Response System, which indicates whether the individual is in default on direct or guaranteed federal loans or are otherwise ineligible for an FHA loan.  Result is transcribed or captured as a screen-print from the HUD website.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CreditCardAuthorization">
				<xsd:annotation>
					<xsd:documentation>Authorizes a company to use a client's credit card for mortgage related charges.  Generally these charges consist of credit report and appraisal fee.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CreditInsuranceAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the rates and charges incurred for credit insurance.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CreditReport">
				<xsd:annotation>
					<xsd:documentation>Describes the borrowers credit history. Provided by a credit reporting bureau, or a service which consolidates reports from multiple sources into a single report (Tri-merge).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CustomerIdentificationForm">
				<xsd:annotation>
					<xsd:documentation>Verifies the identity of any person seeking to open an account; maintains records of the information used to verify the person’s identity; and  determines whether the person appears on any lists of known or suspected terrorists.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DeathCertificate">
				<xsd:annotation>
					<xsd:documentation>Confirms the date, location and cause of a person's death.  Issued by a government official.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DivorceDecree">
				<xsd:annotation>
					<xsd:documentation>Establishes the terms of the divorce.  Provided by a court or legal authority.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ElectronicFundsTransfer">
				<xsd:annotation>
					<xsd:documentation>Establishes a repeatable direct deposit of funds into an account of a borrower. Used by servicing.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EnergyEfficientMortgageWorksheet">
				<xsd:annotation>
					<xsd:documentation>Determines loan amount for Energy Efficient Mortgage loan (loan to finance repairs that increase energy efficiency of the home). Required by HUD.  Provided by Underwriter.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EqualCreditOpportunityActForm">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that all consumers are entitled to an equal chance to obtain credit.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EscrowAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes escrow accounts for taxes and insurance. Agreement between borrower and lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EscrowForCompletionAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes work to be completed on a property and the terms under which the funds for the work will be released.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EscrowForCompletionLetter">
				<xsd:annotation>
					<xsd:documentation>Confirms work on the property has been completed and any funds held in escrow can be released.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EscrowWaiver">
				<xsd:annotation>
					<xsd:documentation>Confirms the lender agrees to waive collecting a monthly payment for taxes and insurance (escrow or impound account). Informs the applicant of their responsibilities to pay tax/insurance bills when due, whether an escrow waiver fee will be imposed, and information regarding lender placement of insurance.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EstimateOfClosingCostsPaidToThirdParty">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of estimated loan closing costs.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EstoppelAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes that certain statements of fact are correct as of the date of the statement and can be relied upon by a third party (including a prospective lender or purchaser).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FACTACreditScoreDisclosure">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of their credit score, range of possible scores, key factors affecting score, reporting agencies used, contacts for obtaining a report copy, and includes a statement that terms offered are based on consumer report information.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FairLendingNotice">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that it is illegal to discriminate in lending practices. This is different that the Equal Credit Opportunity Act.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FederalApplicationInsuranceDisclosure">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that the lender is offering an insurance product or annuity in connection with an extension of credit.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FederalSaleOfInsuranceDisclosure">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that the lender is selling credit insurance as part of the loan transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FHAFiveDayWaiver">
				<xsd:annotation>
					<xsd:documentation>Confirms that the borrower waives the requirement to receive the Notice to Homebuyer five days before closing.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FHALimitedDenialOfParticipationGeneralServicesAdministrationChecklist">
				<xsd:annotation>
					<xsd:documentation>Confirms that a borrower is not recorded on the LDP/GSA (Limited Denial of Participation/General Services Administration) lists, and therefore is eligible for an FHA loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FHAMIPNettingAuthorization">
				<xsd:annotation>
					<xsd:documentation>Determines the appropriate amount for an FHA streamlined refinance loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FHAMortgageCreditAnalysisWorksheet">
				<xsd:annotation>
					<xsd:documentation>Determines the appropriate amount for an FHA loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FHAReferralChecklist">
				<xsd:annotation>
					<xsd:documentation>Confirms that all requirements have been met prior to FHA foreclosure. Provided to FHA.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FHARefinanceMaximumMortgageWorksheet">
				<xsd:annotation>
					<xsd:documentation>Determines loan amount for refinance loan, included with or attached to the MCAW.  Required by HUD, provided by Underwriter.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FinancialStatement">
				<xsd:annotation>
					<xsd:documentation>Describes financial performance of a business or individual.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FloodHazardNotice">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that flood insurance may be required by the lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FloodInsuranceAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes that a borrower will maintain flood insurance in the amount prescribed by the lender for the duration of the loan term.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ForYourProtectionHomeInspection">
				<xsd:annotation>
					<xsd:documentation>Confirms that a borrower has received the information that the borrower should consider a home inspection.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FreddieMacOwnedStreamlineRefinanceChecklist">
				<xsd:annotation>
					<xsd:documentation>Confirms that a loan meets Freddie Mac streamlined refinance criteria. Provided to Freddie Mac.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FundingTransmittal">
				<xsd:annotation>
					<xsd:documentation>Confirms final closing information and numbers on a brokered or third party closing.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="GeneralLoanAcknowledgement">
				<xsd:annotation>
					<xsd:documentation>Confirms receipt of all the disclosures in the Disclosure Booklet,  HUD Guide to Settlement Costs. Also used to document the float/lock rate requests and mortgage insurance requests and provide the Appraisal Notice to the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="GiftLetter">
				<xsd:annotation>
					<xsd:documentation>Confirms that the borrower is receiving all or part of the down payment as a gift.  Provided by gift donor.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="GoodFaithEstimate">
				<xsd:annotation>
					<xsd:documentation>Describes the approximate costs borrower will pay at or before settlement, based on common practice in the locality. The mortgage banker or mortgage broker, if any, must deliver or mail the GFE to the applicant within three business days after the application is received. This document is a part of HUD Regulation X (RESPA/Reg X).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="GroupSavingsAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes participation and terms in a Group Savings Plan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HighCostWorksheet">
				<xsd:annotation>
					<xsd:documentation>Determines whether the loan is high cost or predatory based on state or national statutes.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HoldHarmlessAgreement">
				<xsd:annotation>
					<xsd:documentation>Agrees to hold harmless and indemnify (assumes the legal liability of) the lender from any future liability on items defined in the notice.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeBuyerEducationCertification">
				<xsd:annotation>
					<xsd:documentation>Confirms that the borrower has completed a homebuyer education program.  The program are usually in association with first time homebuyer and affordable housing programs.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeEquityConversionMortgageAntiChurningDisclosure">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that the costs associated with the new HECM loan will not exceed the loan amount.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeEquityConversionMortgageChoiceOfInsuranceOptionsForm">
				<xsd:annotation>
					<xsd:documentation>Confirms that a borrower will choose an FHA Insured HECM loan. Provided by the Lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeEquityConversionMortgageFaceToFaceCertification">
				<xsd:annotation>
					<xsd:documentation>Confirms that a borrower has met with a lending representative, in person, at some point during the HECM loan origination process.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeEquityConversionMortgageCounselingWaiverQualification">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of right to waive HECM counseling requirements if the original loan is originated within five years of the preceding loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeEquityConversionMortgageExtension">
				<xsd:annotation>
					<xsd:documentation>Establishes additional time to sell a property associated with a Home Equity Conversion Mortgage.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeEquityConversionMortgageLoanSubmissionSchedule">
				<xsd:annotation>
					<xsd:documentation>Describes final loan information and figures in a HECM transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeEquityConversionMortgageNoticeToBorrower">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of the steps to take if payment is not received from the HECM lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeEquityConversionMortgagePaymentPlan">
				<xsd:annotation>
					<xsd:documentation>Describes the charges associated with the HECM loan and the payments made to the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeEquityLineFreezeLetter">
				<xsd:annotation>
					<xsd:documentation>Authorizes lender to freeze a Home Equity Line of Credit in anticipation of a pay off.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HomeownersAssociationCertification">
				<xsd:annotation>
					<xsd:documentation>Confirms the property meets HUD requirements. Provided by the Condominium Association.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IdentityAffidavit">
				<xsd:annotation>
					<xsd:documentation>Confirms the correct borrower name to be used in executing the documents.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IdentityTheftDisclosure">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower about identify theft and how to protect one's credit.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IncompleteApplicationNotice">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that the application as submitted is incomplete and establishes a time period in which information must be received.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IndividualDevelopmentAccountStatement">
				<xsd:annotation>
					<xsd:documentation>Describes the funds available through a program that matches funds of the borrower deposited for the sole purpose of purchasing a residence.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="InitialEscrowAccountDisclosure">
				<xsd:annotation>
					<xsd:documentation>Estimates the amount and activity in the escrow account based on anticipated future payments.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="InterestRateAndDiscountDisclosureStatement">
				<xsd:annotation>
					<xsd:documentation>Discloses the interest rate and discount points to the borrower and are an agreement between the borrower and the lender. The Veteran's Affairs does not govern the rate or points on the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Invoice">
				<xsd:annotation>
					<xsd:documentation>Provides bill which itemizes nature and cost of transactions.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IRS1098">
				<xsd:annotation>
					<xsd:documentation>Confirms the amount of interest a person has paid on a mortgage.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IRS1099">
				<xsd:annotation>
					<xsd:documentation>Confirms a person’s non-salary income.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IRSW2">
				<xsd:annotation>
					<xsd:documentation>Confirms a person's wage income and taxes withheld.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IRSW8">
				<xsd:annotation>
					<xsd:documentation>Confirms the individual is a foreign tax resident.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IRSW9">
				<xsd:annotation>
					<xsd:documentation>Provides the borrowers tax identification number and certifies truthfulness of data provided.  Provided by borrower via IRS W9 form.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ItemizationOfAmountFinanced">
				<xsd:annotation>
					<xsd:documentation>Describes the finance charges associated with the mortgage loan. May be included in the Truth in Lending disclosure.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Judgment">
				<xsd:annotation>
					<xsd:documentation>Evidences an obligation determined by a decree of a court.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LandLeaseholdAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the interest in land of a person who owns the lease granted by the property owner.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LastWillAndTestament">
				<xsd:annotation>
					<xsd:documentation>Establishes the rights of others over an individual's (the Testator's) property after the individual's death.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LegalDescription">
				<xsd:annotation>
					<xsd:documentation>Describes the physical location of the property as defined in the country records.  This is primarily a component of other documents (i.e. security instruments, title policies, commitments), but is received separately on some occasions.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LenderCorrespondence">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower or a third party, information from, or about, the lender. This is other then a federal, state, or local requirements or regulations.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LifeInsurancePolicy">
				<xsd:annotation>
					<xsd:documentation>Specifies agreement between a company and an individual to pay a pre-determined amount of money to a beneficiary if the insured dies during the policy term.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LineItemBudget">
				<xsd:annotation>
					<xsd:documentation>Describes in detail the budget for the construction of a home.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LoanApplication">
				<xsd:annotation>
					<xsd:documentation>Provides information about a prospective borrower's employment, income, assets, debts and other financial information.  Includes information about the purpose of the home loan and about the property securing the home loan. Provided by Borrower with lender's assistance.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LoanClosingNotice">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of total finance charges and interest rates associated with the loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LoanPayoffRequest">
				<xsd:annotation>
					<xsd:documentation>Provides Lender with information to determine the amount required to pay off an account and the outstanding principle balance as of a specific date.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LoanStatement">
				<xsd:annotation>
					<xsd:documentation>Describes transactions recorded and account balances of a customers accounts during a specified period.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LoanSubmissionSummary">
				<xsd:annotation>
					<xsd:documentation>Summarizes key loan origination information including; pricing, product, contact and payment information; does not include credit decision.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ManufacturedHousingCertificateOfTitle">
				<xsd:annotation>
					<xsd:documentation>Proves the ownership of the manufactured home and shows any liens on the property. Lists the manufacturer, serial numbers, and date of manufacture.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MarriageCertificate">
				<xsd:annotation>
					<xsd:documentation>Confirms that a marriage has occurred.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MilitaryDischargePapers">
				<xsd:annotation>
					<xsd:documentation>Confirms that a person served in the military and describes their veterans status.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MortgageInsuranceConditionalCommitment">
				<xsd:annotation>
					<xsd:documentation>Establishes a commitment to issue mortgage insurance pending specified conditions.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MortgageInsuranceCertificate">
				<xsd:annotation>
					<xsd:documentation>Confirms existence of the insurance policy protecting the lender against loss from the borrower's failure to make payments on the mortgage loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MortgageInsuranceDisclosure">
				<xsd:annotation>
					<xsd:documentation>Explains terms, requirements, or costs of mortgage insurance.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MortgageInsuranceModification">
				<xsd:annotation>
					<xsd:documentation>Changes one or more of the terms of the Mortgage Insurance Certificate after closing.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NameAffidavit"/>
			<xsd:enumeration value="NearestLivingRelativeInformation">
				<xsd:annotation>
					<xsd:documentation>Identifies the borrower's nearest living relative.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NonDiplomatVerification">
				<xsd:annotation>
					<xsd:documentation>Confirms that a person is not a diplomat, in the case where the person is a non-resident alien.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Note">
				<xsd:annotation>
					<xsd:documentation>Establishes a legal obligation of the borrower to repay the debt secured by the mortgage.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoteAddendum">
				<xsd:annotation>
					<xsd:documentation>Adds the specific terms of the ARM to the promissory note.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoteAllonge">
				<xsd:annotation>
					<xsd:documentation>Adds additional page(s) to the Note for endorsements.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoteAndSecurityInstrumentModification">
				<xsd:annotation>
					<xsd:documentation>Changes one or more of the terms of a mortgagor's loan in the note and in the security instrument.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoteModification">
				<xsd:annotation>
					<xsd:documentation>Changes one or more of the terms of the loan in the promissory note.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoticeOfActionTaken">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of denial or other material change in the loan application.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoticeOfCompletion">
				<xsd:annotation>
					<xsd:documentation>Changes (updates) a previous appraisal that was prepared subject to completion of repairs or incomplete items.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoticeOfRightToCancel">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of their three-day right to cancel the loan application.  Required by TILA / Reg Z.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoticeOfRightToCopyOfAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of their right to a copy of their appraisal report.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoticeToHomebuyer">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of the repair conditions listed on the Notice to Lender form, outlining any issues or comments on the appraisal. Provided by the lender to the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NoticeToLender">
				<xsd:annotation>
					<xsd:documentation>Describes repair conditions on the subject property. Provided by Appraiser to the lender. Provided as an addendum to Appraisal.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OccupancyAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the borrower intends to occupy the property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OccupancyCertification">
				<xsd:annotation>
					<xsd:documentation>Confirms that a property is fit for occupancy.  Provided by local municipal authority.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>To be used with TypeOtherDescription data point to describe a document not already on this list.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PartialReleaseOfSecurityInstrument">
				<xsd:annotation>
					<xsd:documentation>Releases a specific parcel of real property from an existing security instrument.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PartnershipAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the terms of a partnership, including the rights and responsibilities of the partners.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PaymentHistory">
				<xsd:annotation>
					<xsd:documentation>Describes history of payments on a loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PaymentLetter">
				<xsd:annotation>
					<xsd:documentation>Describes the terms of a loan and loan payments, often emphasizing timing and amount of the first payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PayoffStatement">
				<xsd:annotation>
					<xsd:documentation>Confirms the remaining principle balance of a loan that is being paid off or refinanced, based on a defined close or pay date.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PayStub">
				<xsd:annotation>
					<xsd:documentation>Describes income for a particular pay period.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PermanentResidentIdentification">
				<xsd:annotation>
					<xsd:documentation>Confirms a person's permanent legal residency.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PersonalIdentification">
				<xsd:annotation>
					<xsd:documentation>Confirms a person's identity for the purposes of a government-regulated activity (such as driving or international travel).  Provided by the controlling governmental authority.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PersonalPropertyAppraisalReport">
				<xsd:annotation>
					<xsd:documentation>Describes personal property and provides and estimation of value.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PowerOfAttorney">
				<xsd:annotation>
					<xsd:documentation>Authorizes one person to act as agent or attorney on behalf of another person.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PreApplicationDisclosureNotice">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of appraisal and credit report fees. A state disclosure.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PrequalificationLetter">
				<xsd:annotation>
					<xsd:documentation>Informs a perspective borrowers that they will qualify for a loan based on the income and credit information the borrower has provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PrepaymentChargeOptionNotification">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of "premium pricing option" (i.e., option of prepayment charge in lieu of  higher interest rate).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PrivacyDisclosure">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of privacy rights; describes what information will be collected and shared.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PropertyInspectionReport">
				<xsd:annotation>
					<xsd:documentation>Describes the condition of the subject property, or any of a variety of building systems or interior or exterior elements of the structure, or aspects of the surrounding property (such as water or soil).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PropertyInsuranceBinder">
				<xsd:annotation>
					<xsd:documentation>Establishes a commitment to issue property insurance pending payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PropertyInsuranceDeclarationsPage">
				<xsd:annotation>
					<xsd:documentation>Describes the information regarding the risk covered by the insurance policy, including the name of the insurance company,  its address, name of the policy holder, starting and ending dates of coverage, the actual coverage given in the contract, including locations and amounts.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PropertyInsurancePolicy">
				<xsd:annotation>
					<xsd:documentation>Establishes the terms and conditions of the contract for insurance covering the subject property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PropertyInsuranceRequirementDisclosure">
				<xsd:annotation>
					<xsd:documentation>Explains lenders right to approve a borrower's choice of insurance company and the required specifics of the insurance such as deductible, premium and term.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PurchaseAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the terms and conditions for purchase of a property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RateLockAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes that the lender and borrower have fixed (locked) specific terms of the loan application (including points, fees, expiration and product type) for a specified period.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ReaffirmationAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes that the borrower will reinstitute a mortgage debt after bankruptcy proceedings are completed.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Receipt">
				<xsd:annotation>
					<xsd:documentation>Confirms the payment or exchange of a good or service.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Reconveyance">
				<xsd:annotation>
					<xsd:documentation>Conveys a trustee's interest back to the property owner when a deed of trust is paid in full. In some jurisdictions both a Satisfaction of Deed of Trust and a Reconveyance.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RelocationBenefitsPackage">
				<xsd:annotation>
					<xsd:documentation>Describes terms and conditions of the relocation package.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RelocationBuyoutAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the terms between a relocation company and an employee to purchase a home based on the appraised value. It describes what happens if a home does not sell within a certain period (usually 60 or 90 days), and the circumstances in which a relocation company will  buy a home.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RentalAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the rights and duties of the landlord and tenant in a rental transaction. At a minimum identifies the parties, the property, the term of the rental, and the amount of rent for the term.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RentalIncomeAnalysisStatement">
				<xsd:annotation>
					<xsd:documentation>Evaluates the rental potential of a property and establishes its value.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RequestForCopyOfTaxReturn">
				<xsd:annotation>
					<xsd:documentation>Authorizes taxing authority (such as IRS) to provide certified copy of borrowers tax return.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RequestForNoticeOfDefault">
				<xsd:annotation>
					<xsd:documentation>Establishes a requirement for a senior lien holder to inform a junior lien holder in the event of a default on the common property. Required to be recorded.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RoadMaintenanceAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the parties and their respective responsibilities to jointly maintain a road.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SatisfactionOfJudgment">
				<xsd:annotation>
					<xsd:documentation>Confirms that a judgment filed against a property has been released. Provided by a legal authority.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SatisfactionOfSecurityInstrument">
				<xsd:annotation>
					<xsd:documentation>Confirms the security interest has been paid off and the lien on the property is released. Required to be recorded.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Section32DisclosureForm">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that a loan is considered a high cost loan.  Required by Regulation Z.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SecurityInstrument">
				<xsd:annotation>
					<xsd:documentation>Establishes the  lender’s security interest in the real property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SecurityInstrumentModification">
				<xsd:annotation>
					<xsd:documentation>Changes one or more of the terms of a mortgagor's loan in the security instrument.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SecurityInstrumentRider">
				<xsd:annotation>
					<xsd:documentation>Adds additional provisions to the security instrument.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ServicingDisclosureStatement">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that the right to collect their mortgage loan payments may be transferred, and they have certain rights under federal law. Required by RESPA / Regulation X.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ServicingTransferStatement">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower that the servicing of their loan is being transferred. Required by RESPA / Regulation X.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SettlementStatement">
				<xsd:annotation>
					<xsd:documentation>Describes the funds that are payable at closing. Items that appear on the statement include real estate commissions, loan fees, points, and initial escrow amounts.  Required by RESPA / Regulation X.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SocialSecurityAwardLetter">
				<xsd:annotation>
					<xsd:documentation>Confirms that Social Security benefits are payable and the amount received each month.  Provided by the Social Security Administration.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="StandardFloodHazardDetermination">
				<xsd:annotation>
					<xsd:documentation>Confirms whether the property is located in a flood zone and therefore whether it requires flood insurance.  Provided by FEMA.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="StatementOfBorrowerBenefit">
				<xsd:annotation>
					<xsd:documentation>Describes how the borrower is benefiting from a loan transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="StockCertificate">
				<xsd:annotation>
					<xsd:documentation>Confirms legal ownership of stock in a corporation.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SubordinationAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes that a prior lien is made inferior to an otherwise junior lien.  An Agreement between two creditors (lien holders) of a particular borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SubsidyAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes that the borrower will set up a subsidy account to supplement the mortgage payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SubstitutionOfTrustee">
				<xsd:annotation>
					<xsd:documentation>Names a successor trustee.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Survey">
				<xsd:annotation>
					<xsd:documentation>Describes encroachments on mortgaged property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SurveyAffidavit">
				<xsd:annotation>
					<xsd:documentation>Confirms that no encroachments have been built since the preceding survey. Provided by the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TaxAuthorization">
				<xsd:annotation>
					<xsd:documentation>Authorizes the tax collector to send the tax bill to the respective mortgagee, servicing or processing organization.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TaxCertificate">
				<xsd:annotation>
					<xsd:documentation>Describes taxes charged by state or local governments.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TaxLien">
				<xsd:annotation>
					<xsd:documentation>Evidences a tax lien against a property, person, or entity.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TaxLienRelease">
				<xsd:annotation>
					<xsd:documentation>Releases a tax lien against a property, person, or entity.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TaxReturn">
				<xsd:annotation>
					<xsd:documentation>Provides information used to calculate income or other taxes.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TenYearWarranty">
				<xsd:annotation>
					<xsd:documentation>Confirms existence of the insurance policy that covers the property against defects in workmanship. Provided by Insurance company.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TitleAbstract">
				<xsd:annotation>
					<xsd:documentation>Describes the result of a title search of public records.  This is the source document that produces the conditions in a title commitment.  Provided by Title Insurance company.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TitleCommitment">
				<xsd:annotation>
					<xsd:documentation>Establishes a commitment to issue Title insurance subject to payment or other specified conditions. Provided by the Title Insurance company.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TitleInsuranceEndorsement">
				<xsd:annotation>
					<xsd:documentation>Changes one or more of the terms of the Title Insurance Policy.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TitleInsurancePolicy">
				<xsd:annotation>
					<xsd:documentation>Establishes that the insurer agrees to pay the insured (a purchaser or mortgagee with an interest in the property) a specific amount for any loss caused by defects of title to the real estate.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TruthInLendingDisclosure">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of the annual percentage rate, or APR, of the borrower's loan as well as other facets of the mortgage program.  Required by TILA / Reg Z.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TrustAgreement">
				<xsd:annotation>
					<xsd:documentation>Establishes the terms of a Trust, including the rights and responsibilities of the trustee and the trustor.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UCC1Statement">
				<xsd:annotation>
					<xsd:documentation>Describes property taken as collateral from a borrower.  Required to be filed in public records to record security interest in the property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UCC3Statement">
				<xsd:annotation>
					<xsd:documentation>Changes the UCC-1 by amending or terminating the security interest.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UnderwritingTransmittal">
				<xsd:annotation>
					<xsd:documentation>Describes key information utilized in the comprehensive risk assessment of the mortgage, and the final underwriting decision.  Used for manually-processed applications for single-family conventional first and second mortgages.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VAAcknowledgementOfNoInspection">
				<xsd:annotation>
					<xsd:documentation>Acknowledges the borrower's awareness that the property was not inspected and will not qualify for government assistance in the correction of structural defects.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VACertificateOfEligibility">
				<xsd:annotation>
					<xsd:documentation>Confirms a person's veteran status and prior home loan usage.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VACertificateOfReasonableValue">
				<xsd:annotation>
					<xsd:documentation>Confirms the maximum value and loan amount for a VA mortgage.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VACollectionPolicyNotice">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of the actions that can be taken by the VA if a borrower defaults on a loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VAForeclosureBidLetter">
				<xsd:annotation>
					<xsd:documentation>Describes the procedure for purchasing a foreclosed VA property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VAFundingFeeNotice">
				<xsd:annotation>
					<xsd:documentation>Informs the borrower of VA funding fee costs.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VAInterestRateReductionRefinancingLoanWorksheet">
				<xsd:annotation>
					<xsd:documentation>Determines amount current VA homeowners can reduce their interest rate with no out of pocket expenses and minimal documentation.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VALoanAnalysis">
				<xsd:annotation>
					<xsd:documentation>Determines whether a veteran qualifies for a VA loan and whether a lenders adheres to VA standards.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ValuationUpdate">
				<xsd:annotation>
					<xsd:documentation>Details an update or correction to an appraisal report.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VARateReductionRefinanceInformationAndAgreement">
				<xsd:annotation>
					<xsd:documentation>Acknowledges the borrower is aware of the terms of the existing loan and the terms of&#13;
the new loan, whether the payment will be increasing or decreasing and how long it takes to recoup all closing costs.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VAReportAndCertificationOfLoanDisbursement">
				<xsd:annotation>
					<xsd:documentation>Describes details of a VA closed loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VARequestForCertificationOfEligibilityForHomeLoanBenefit">
				<xsd:annotation>
					<xsd:documentation>Provides VA with information to make determination of a borrower's VA eligibility.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VAVerificationOfBenefitRelatedIndebtedness">
				<xsd:annotation>
					<xsd:documentation>Confirms whether a veteran has the ability to incur debt on a loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VerificationOfCredit">
				<xsd:annotation>
					<xsd:documentation>Confirms borrower credit information.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VerificationOfDependentCare">
				<xsd:annotation>
					<xsd:documentation>Confirms dependent care expenses paid by borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VerificationOfDeposit">
				<xsd:annotation>
					<xsd:documentation>Confirms borrowers deposit information.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VerificationOfEmployment">
				<xsd:annotation>
					<xsd:documentation>Confirms borrowers employment information.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VerificationOfMortgageOrRent">
				<xsd:annotation>
					<xsd:documentation>Confirms mortgage or rent expenses paid by borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VerificationOfSecurities">
				<xsd:annotation>
					<xsd:documentation>Confirms information about securities owned by borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VolunteerEscrowPrepaymentDesignation">
				<xsd:annotation>
					<xsd:documentation>Authorizes lender to increase borrower's escrow accounts.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WireInstructions">
				<xsd:annotation>
					<xsd:documentation>Describes the procedure for wiring funds to escrow account. Provided by closing agent.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WireTransferAuthorization">
				<xsd:annotation>
					<xsd:documentation>Authorizes a wire transfer of funds.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WireTransferConfirmation">
				<xsd:annotation>
					<xsd:documentation>Confirms that the recipient is in receipt of funds sent via bank wire process.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Worksheet">
				<xsd:annotation>
					<xsd:documentation>Describes the procedure for performing an analysis or making a determination and captures results of the work.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DocumentationStateTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Identifies the state of the documention associated with the loan.</xsd:documentation>
			<xsd:documentation>term: Documentation State Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AsRequested">
				<xsd:annotation>
					<xsd:documentation>As requested by the borrower</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AsCollected">
				<xsd:annotation>
					<xsd:documentation>As collected from the borrower</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AsRequired">
				<xsd:annotation>
					<xsd:documentation>As required of the borrower</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DownPaymentOptionTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the type of option used to facilitate the borrowers down payment.</xsd:documentation>
			<xsd:appinfo>  AUS  Enumerated Loan Features  AUS  Down Payment Option Type </xsd:appinfo>
			<xsd:documentation>term: Down Payment Option Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ThreeTwoOption"/>
			<xsd:enumeration value="FivePercentOption"/>
			<xsd:enumeration value="FNM97Option"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DownPaymentSourceTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the entity providing funds for the down payment.</xsd:documentation>
			<xsd:documentation>term: Down Payment Source Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Borrower"/>
			<xsd:enumeration value="Builder"/>
			<xsd:enumeration value="CommunityNonProfit"/>
			<xsd:enumeration value="Employer"/>
			<xsd:enumeration value="FederalAgency"/>
			<xsd:enumeration value="LocalAgency"/>
			<xsd:enumeration value="NonOriginatingFinancingInstitution"/>
			<xsd:enumeration value="OriginatingLender"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="OwnerInvestor"/>
			<xsd:enumeration value="Relative"/>
			<xsd:enumeration value="ReligiousNonProfit"/>
			<xsd:enumeration value="StateAgency"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DownPaymentTypeEnumerated">
		<xsd:annotation>
			<xsd:documentation>Specifies the general names (type) of items commonly used for a down payment by the borrower(s) in a mortgage loan transaction. This may be collected on the URLA in Section II (Source of Down Payment).</xsd:documentation>
			<xsd:appinfo>  AUS  Enumerated Down Payment  Secondary Delivery AUS Secondary Pricing  Down Payment Type </xsd:appinfo>
			<xsd:documentation>term: Down Payment Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BridgeLoan"/>
			<xsd:enumeration value="CashOnHand"/>
			<xsd:enumeration value="CreditCard"/>
			<xsd:enumeration value="DepositOnSalesContract"/>
			<xsd:enumeration value="EquityOnPendingSale"/>
			<xsd:enumeration value="EquityOnSoldProperty"/>
			<xsd:enumeration value="EquityOnSubjectProperty"/>
			<xsd:enumeration value="GiftFunds"/>
