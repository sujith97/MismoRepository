			<xsd:element name="HAZARD_INSURANCE" type="HAZARD_INSURANCE" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Information specific to any hazard insurance policy on the property that secures the loan. There may be multiple policies, such as homeowners, fire and extended coverage, flood, wind, or earthquake insurance.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="HELOC">
		<xsd:sequence>
			<xsd:element name="HELOC_OCCURRENCES" type="HELOC_OCCURRENCES" minOccurs="0"/>
			<xsd:element name="HELOC_RULE" type="HELOC_RULE" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="HELOC_OCCURRENCE">
		<xsd:sequence>
			<xsd:element name="CurrentHELOCMaximumBalanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The total dollar amount of the line of credit as of the date reported.</xsd:documentation>
					<xsd:appinfo>Renamed.  Used to be "Current Credit Line Maximum Amount".</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCBalanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The outstanding balance of the home equity line of credit (HELOC).</xsd:documentation>
					<xsd:appinfo>Renamed.  Used to be "Subordinate Lien HELOC Amount".</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCDailyPeriodicInterestRatePercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The interest rate percent used to calculate the dollar amount of the daily periodic interest charged in the HELOC loan transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCTeaserTermEndDate" type="MISMODate" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date when the discounted interest rate is no longer used and the interest rate is determined using the index and margin.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
		<xsd:attribute name="SequenceNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>An integer value used to provide an order to multi-instance sibling elements.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="HELOC_OCCURRENCES">
		<xsd:sequence>
			<xsd:element name="HELOC_OCCURRENCE" type="HELOC_OCCURRENCE" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="HELOC_RULE">
		<xsd:sequence>
			<xsd:element name="HELOCAnnualFeeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The dollar amount of an annual fee associated with the loan and charged to the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCCreditCardAccountIdentifier" type="MISMOIdentifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The account number assigned for the HELOC credit card.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCCreditCardIndicator" type="MISMOIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the loan payments are to be charged to a credit card account.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCDailyPeriodicInterestRateCalculationType" type="HELOCDailyPeriodicInterestRateCalculationTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The factor used in calculating the dollar amount of daily periodic interest charged in the HELOC loan transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCInitialAdvanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount of money that a borrower receives at closing in a HELOC transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCMaximumAPRPercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The maximum annual percentage rate (APR) that can be charged in association with the HELOC loan transaction.</xsd:documentation>
					<xsd:appinfo>Renamed.  Used to be "HELOC Maximum APR Rate Percent".</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCMaximumBalanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The maximum dollar amount of credit available to the borrower on a home equity line of credit (HELOC), regardless of whether the borrower has accessed the amount.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCMinimumAdvanceAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The minimum amount that will be allowed as an advance from the total Credit limit in a HELOC transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCMinimumPaymentAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The minimum dollar amount that can be paid as a monthly payment for the HELOC loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCMinimumPaymentPercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The minimum percentage rate that can be used to calculate the monthly payment for the HELOC loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCRepayPeriodMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The term in months by which full payment of the HELOC loan balance must be made.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCReturnedCheckChargeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount that can be charged in the case of a payment made on the HELOC loan being returned for non sufficient funds.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCStopPaymentChargeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount that can be charged when a stop payment is made on a loan HELOC payment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCTeaserMarginRatePercent" type="MISMOPercent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The margin rate, expressed as a percent, stated on the line of credit with a variable introductory (teaser) rate.</xsd:documentation>
					<xsd:appinfo>Renamed.  Used to be "HELOC Teaser Margin Rate".</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCTeaserRateType" type="HELOCTeaserRateTypeEnumerated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of introductory (teaser) rate on a line of credit.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCTeaserTermMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of months the discounted interest rate is used to determine the payment amount.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCTerminationFeeAmount" type="MISMOAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The fee amount charged when the HELOC transaction is paid off early.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HELOCTerminationPeriodMonthsCount" type="MISMOCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of months until the HELOC is terminated.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
	</xsd:complexType>
	<xsd:complexType name="HMDA_LOAN">
