			<xsd:element name="FLOOD_REQUEST_DETAIL" type="FLOOD_REQUEST_DETAIL" minOccurs="0"/>
			<xsd:element name="FLOOD_REQUEST_DISPUTE" type="FLOOD_REQUEST_DISPUTE" minOccurs="0"/>
			<xsd:element name="EXTENSION" type="EXTENSION" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attributeGroup ref="xlink:resourceLink"/>
