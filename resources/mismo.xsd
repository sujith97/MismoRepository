	<xsd:complexType name="DEAL">
		<xsd:annotation>
			<xsd:documentation>The root element for all transactions that apply to the use cases of a single deal.  A Deal may contain multiple loans or loan applications.</xsd:documentation>
		</xsd:annotation>
