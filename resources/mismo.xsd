			<xsd:appinfo>  SEC  Enumerated Escrow Hazard Insurance Item  Secondary Delivery Servicing Servicing Transfer SEC  Hazard Insurance Non Standard Policy Type </xsd:appinfo>
			<xsd:documentation>term: Hazard Insurance Non Standard Policy Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Application"/>
			<xsd:enumeration value="Binder"/>
			<xsd:enumeration value="Bridge"/>
			<xsd:enumeration value="Condo"/>
			<xsd:enumeration value="FairPlan"/>
			<xsd:enumeration value="ForcePlaced"/>
